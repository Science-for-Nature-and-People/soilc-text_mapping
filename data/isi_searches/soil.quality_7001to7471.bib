@article{ ISI:000075049900007,
Author = {Andreev, GG and Afanas'eva, TV and Chaban, LN},
Title = {{Automated thematic processing of multiband space images of floodplains
   in generation of soil maps and estimation of the quality of floodplain
   lands}},
Journal = {{EARTH OBSERVATION AND REMOTE SENSING}},
Year = {{1998}},
Volume = {{15}},
Number = {{1}},
