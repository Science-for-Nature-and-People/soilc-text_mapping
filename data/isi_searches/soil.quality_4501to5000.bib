@article{ ISI:000276704500004,
Author = {Arthurson, Veronica},
Title = {{Closing the Global Energy and Nutrient Cycles through Application of
   Biogas Residue to Agricultural Land - Potential Benefits and Drawbacks}},
Journal = {{ENERGIES}},
Year = {{2009}},
Volume = {{2}},
Number = {{2}},
Pages = {{226-242}},
Month = {{JUN}},
Abstract = {{Anaerobic digestion is an optimal way to treat organic waste matter,
   resulting in biogas and residue. Utilization of the residue as a crop
   fertilizer should enhance crop yield and soil fertility, promoting
   closure of the global energy and nutrient cycles. Consequently, the
   requirement for production of inorganic fertilizers will decrease, in
   turn saving significant amounts of energy, reducing greenhouse gas
   emissions to the atmosphere, and indirectly leading to global economic
   benefits. However, application of this residue to agricultural land
   requires careful monitoring to detect amendments in soil quality at the
   early stages.}},
