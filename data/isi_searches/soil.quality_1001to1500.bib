@article{ ISI:000211675200026,
Author = {Gour, S. and Singh, S. and Lal, Rattan and Singh, R. and Bohra, J. and
   Srivastava, J. and Singh, S. and Kumar, Maneesh and Kumar, Omkar and
   Latare, A.},
Title = {{Effect of organic and inorganic sources of plant nutrients on growth and
   yield of rice (Oryza sativa) and soil fertility}},
Journal = {{INDIAN JOURNAL OF AGRONOMY}},
Year = {{2015}},
Volume = {{60}},
Number = {{2}},
Pages = {{328-331}},
Month = {{JUN}},
Abstract = {{A field experiment was conducted in randomized block design during the
   rainy (kharit) season of 2012 at Varanasi, Uttar Pradesh to study the
   effect of organic and inorganic sources of nutrients on soil quality and
   productivity of rice (Oryza sativa L.). The highest grain and straw
   yields (5.03 and 7.18 t/ha) were recorded under the treatments 100\%
   recommended dose of fertilizer (RDF) + S-Zn-B and 75\% RDF + 25\% N
   through sewage sludge, respectively. Application of S (40 kg/ha), Zn (5
   kg/ha) and B (1.5 kg/ha) along with NPK (120:60:60) in rice increased
   plant height, tiller number, grain yield and 1,000-grian weight 22, 99,
   62 and 22\%, respectively, over the control, but did not show any
   significant increase in these parameters over 100\% RDF. Initial trends
   indicated that application of Sesbania perhaps increased N, P, B and Zn
   in soil while inorganic S, B and Zn fertilizers had residual effect in
   post-harvest soil.}},
