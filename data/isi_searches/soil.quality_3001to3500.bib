@article{ ISI:000299328300014,
Author = {Diaz Rizo, O. and Hernandez Merlo, M. and Echeverria Castillo, F. and
   Arado Lopez, J. O.},
Title = {{Assessment of Metal Pollution in Soils From a Former Havana (Cuba) Solid
   Waste Open Dump}},
Journal = {{BULLETIN OF ENVIRONMENTAL CONTAMINATION AND TOXICOLOGY}},
Year = {{2012}},
Volume = {{88}},
Number = {{2}},
Pages = {{182-186}},
Month = {{FEB}},
Abstract = {{Concentrations of cobalt, nickel, cooper, zinc and lead in the top-soils
   (0-10 cm) from a former Havana solid waste open dump were estimated by
   X-ray fluorescence analysis. The mean metal contents in the dump topsoil
   samples (in mg kg(-1): 8.4 +/- A 2.7 for cobalt, 50 +/- A 27 for nickel,
   252 +/- A 80 for copper, 489 +/- A 230 for zinc and 276 +/- A 140 for
   lead) were compared with mean concentrations from Havana urban soils and
   from other solid waste disposals around the world. The comparison with
   Dutch soil quality guidelines showed a serious cooper contamination and
   a slight contamination with the rest of determined metals. The values of
   the integrated pollution index (mean index = 3.5) indicated that dump
   soils are highly contaminated by metals, and the enrichment index values
   shows that metal concentrations on the studied locations are above the
   permissible levels for urban agriculture.}},
