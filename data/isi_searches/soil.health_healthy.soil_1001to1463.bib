@article{ ISI:000364223200001,
Author = {Murtaza, Ghulam and Murtaza, Behzad and Anwar-Ul-Hassan},
Title = {{Management of Low-Quality Water on Marginal Salt-Affected Soils with
   Wheat and Sesbania Crops}},
Journal = {{COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS}},
