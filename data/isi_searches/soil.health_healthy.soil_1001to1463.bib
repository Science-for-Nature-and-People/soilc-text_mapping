@inproceedings{ ISI:000385238300127,
Author = {Koleva, L. and Tsolova, E.},
Editor = {{Milatovic, D and Milivojevic, J and Nikolic, D}},
Title = {{Population structure of free-living and plant-parasitic nematodes in
   strawberry plantations under organic farming conditions}},
Booktitle = {{III BALKAN SYMPOSIUM ON FRUIT GROWING}},
