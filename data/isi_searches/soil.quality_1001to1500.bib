@article{ ISI:000381779500029,
Author = {Yazici, Nilufer and Turan, Aysen},
Title = {{EFFECT OF FORESTRY AFFORESTATION ON SOME SOIL PROPERTIES: A CASE STUDY
   FROM TURKEY}},
