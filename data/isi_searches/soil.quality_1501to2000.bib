Title = {{INDICATOR INDEXING METHODS IN ASSESSMENT OF SOIL QUALITY IN RELATION TO
   WATER EROSION}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
Year = {{2015}},
Volume = {{39}},
