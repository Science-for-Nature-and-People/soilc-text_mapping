@article{ ISI:000326349400005,
Author = {Fterich, Amira and Mahdhi, Mosbah and Mars, Mohamed},
Title = {{Seasonal Changes of Microbiological Properties in Steppe Soils from
   Degraded Arid Area in Tunisia}},
Journal = {{ARID LAND RESEARCH AND MANAGEMENT}},
