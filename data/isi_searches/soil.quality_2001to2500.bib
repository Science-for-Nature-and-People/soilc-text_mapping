@article{ ISI:000331855900009,
Author = {Giacometti, Caterina and Cavani, Luciano and Baldoni, Guido and
   Ciavatta, Claudio and Marzadori, Claudio and Kandeler, Ellen},
Title = {{Microplate-scale fluorometric soil enzyme assays as tools to assess soil
   quality in a long-term agricultural field experiment}},
Journal = {{APPLIED SOIL ECOLOGY}},
Year = {{2014}},
Volume = {{75}},
Pages = {{80-85}},
Month = {{MAR}},
Abstract = {{We investigated the potential of microplate-scale fluorometric soil
   enzyme assays to differentiate plots under contrasting long-term organic
   and mineral N fertilization regimens to determine the relevance of this
   analytical approach to soil quality related studies.
   Enzymes involved in the breakdown of cellulose and hemicellulose showed
   maximum activities in plots amended with manure. Conversely, the enzymes
   involved in the hydrolysis of starch and phosphate esters peaked under
   mineral N fertilization. Linear regression analysis indicated close
   associations between enzyme activities and other fundamental soil
   properties related to soil quality, and principal component analysis
   separated the soil samples according to their responses to organic and
   mineral N fertilization. We conclude that microplate-scale fluorimetry
   is a fast throughput tool for the measurement of multiple soil enzyme
   activities as soil quality indicators. (C) 2013 Elsevier B.V. All rights
   reserved.}},
