@article{ ISI:000344813200003,
Author = {Moore, T. L. and Craig, M. D. and Valentine, L. E. and Hardy, G. E. St
   J. and Fleming, P. A.},
Title = {{Signs of wildlife activity and Eucalyptus wandoo condition}},
Journal = {{AUSTRALIAN MAMMALOGY}},
