@article{ ISI:000371918800004,
Author = {Kennedy, N. and Amacher, G. S. and Alexandre, R.},
Title = {{Adoption of soil and water conservation practices in central Haiti}},
Journal = {{JOURNAL OF SOIL AND WATER CONSERVATION}},
Year = {{2016}},
Volume = {{71}},
