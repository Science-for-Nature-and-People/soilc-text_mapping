@article{ ISI:000171844100006,
Author = {Maly, S and Sarapatka, B and Nemec, P},
Title = {{Interactions among anaerobic and aerobic N mineralization, nitrification
   and selected soil properties}},
Journal = {{ROSTLINNA VYROBA}},
Year = {{2001}},
Volume = {{47}},
Number = {{9}},
Pages = {{405-410}},
Month = {{SEP}},
Abstract = {{Nitrogen mineralization and nitrification are important soil quality
   parameters. The aim of our research was to determine the dependence of
   the microbial parameters which characterize N mineralization and
   nitrification on selected physical and chemical soil properties. The
   results from soil monitoring representing the major soil types of the
   Czech Republic show, that N mineralization measured under aerobic and
   anaerobic conditions was checked by the concentration of organic matter
   (N-tot, C-org). Aerobic N mineralization decreased with increasing
   cation exchange capacity. The values of cation exchange capacity and pH
   influenced the level of nitrification in our research.}},
