@article{ ISI:000286331100022,
Author = {Adewole, M. B. and Anyahara, U. C.},
Title = {{Adoption rate of land clearing techniques and their effects on some soil
   fertility parameters of an Alfisol in southwestern Nigeria}},
Journal = {{AFRICAN JOURNAL OF AGRICULTURAL RESEARCH}},
