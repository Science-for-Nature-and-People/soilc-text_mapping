@inproceedings{ ISI:000178403800039,
Author = {da Silva, AP and Imhoff, S and Corsi, M},
Editor = {{Pagliai, M and Jones, R}},
Title = {{Soil compaction versus cow-stocking rates on an irrigated grazing system}},
Booktitle = {{SUSTAINABLE LAND MANAGEMENT - ENVIRONMENTAL PROTECTION: A SOIL PHYSICAL
   APPROACH}},
Series = {{ADVANCES IN GEOECOLOGY}},
