Editor = {{Brebbia, CA and Popov, V}},
Title = {{Soil erodibility assessment in a pasture and forest remnant using the
   Inderbitzen device}},
Booktitle = {{FOOD AND ENVIRONMENT II: THE QUEST FOR A SUSTAINABLE FUTURE}},
Series = {{WIT Transactions on Ecology and the Environment}},
