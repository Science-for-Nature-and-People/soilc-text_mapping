@article{ ISI:000390829800006,
Author = {Ambarish, Cheviri Nair and Sridhar, Kandikere Ramaiah},
Title = {{Chemical and Microbial Characterization of Feed and Faeces of Two Giant
   Pill-Millipedes from Forests in the Western Ghats of India}},
Journal = {{PEDOSPHERE}},
