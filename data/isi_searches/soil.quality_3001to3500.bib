@article{ ISI:000303082400004,
Author = {Egodawatta, W. C. P. and Sangakkara, U. R. and Stamp, P.},
Title = {{Impact of green manure and mineral fertilizer inputs on soil organic
   matter and crop productivity in a sloping landscape of Sri Lanka}},
Journal = {{FIELD CROPS RESEARCH}},
