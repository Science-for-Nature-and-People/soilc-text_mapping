@article{ ISI:000208665800004,
Author = {Leite, Luiz F. C. and Galvao, Sandra R. S. and Holanda Neto, Manoel R.
   and Araujo, Fernando S. and Iwata, Bruna F.},
Title = {{Chemical attributes and carbon stocks in an Oxisol under no-tillage in
   savannah of Piaui state}},
Journal = {{REVISTA BRASILEIRA DE ENGENHARIA AGRICOLA E AMBIENTAL}},
Year = {{2010}},
Volume = {{14}},
