@article{ ISI:A1991GX39300002,
Author = {ZERULLA, W and KNITTEL, H},
Title = {{YIELD AND QUALITY OF ROOT CROPS AFTER APPLICATION OF DICYANDIAMIDE
   CONTAINING FERTILIZERS .1. COMMUNICATION - INFLUENCE ON POTATOES}},
Journal = {{AGRIBIOLOGICAL RESEARCH-ZEITSCHRIFT FUR AGRARBIOLOGIE AGRIKULTURCHEMIE
   OKOLOGIE}},
Year = {{1991}},
Volume = {{44}},
Number = {{4}},
Pages = {{278-281}},
Abstract = {{When using fertilizers which contain dicyandiamide, the risk of
   fertilizer nitrate being translocated in the first weeks after
   application is significantly reduced.  In addition, the specific mode of
   action of these fertilizers has advantages for plant production.  Trials
   over several years with potato gave the following results:  1. 
   Potatoes, mainly grown on light soils, generally gave increased tuber
   yields following the application of an N-fertilizer containing
   dicyandiamide, compared to a conventional N-fertilizer (+ 2.4 t/ha, mean
   of 12 trials).
   2.  In addition to higher tuber yield, starch content of the tubers was
   increased by 0.2 \% and nitrate content decreased by 39 ppm to 66 ppm on
   average.  The higher N-uptake, compared to ammonium sulfate nitrate,
   resulted in lower N(min)-contents after harvest.
   3.  The effect on yield and quality of the potato crop was largely
   site-independent, there was no relation between additional yield and
   soil quality or precipitation rate at the site of the trial.}},
