@article{ ISI:A1994NY66500003,
Author = {SMITH, EG and JOHNSTON, AM and JANZEN, HH},
Title = {{INFLUENCE OF ORGANIC AMENDMENTS ON THE ECONOMIC-PERFORMANCE OF LONG-TERM
   SPRING WHEAT ROTATIONS IN SOUTHERN ALBERTA}},
Journal = {{JOURNAL OF SUSTAINABLE AGRICULTURE}},
Year = {{1994}},
Volume = {{4}},
Number = {{3}},
Pages = {{31-45}},
Abstract = {{The economic performance of organic amendments (including perennial
   hay), which contribute to maintaining soil quality, in spring wheat
   rotations was evaluated using data from a long-term rotation established
   in 1951 on Dark Brown Chernozem (Typic Haploboroll) soil at Lethbridge,
   Alberta. Net farm income was determined for six rotations over 31 years
   (1954-1984) and four replicates, using actual yields and 1992 prices.
   The six rotations included continuous wheat (W), fallow-wheat (FW),
   fallow-wheat-wheat (FWW), FWW with cattle manure applied during fallow
   (F(M)WW), fallow-wheat-grass/alfalfa hay (FWWAAA), and
   fallow-wheat-grass hay (FWWGGG).  None of the treatments received
   commercial fertilizer.  At a hay price of \$65 t-1 and a low wheat price
   of \$90 t-1, rotation ranking by net farm income (NFI) was FWWAAA >
   FWWGGG = FW > FWW> F(M)WW > W. At a wheat price of \$145 t-1, ranking by
   NFI was FW = FWW = FWWAAA = F(M)WW > FWWGGG > W. At a high wheat price
   of \$200 t-1, rotation ranking by NFI was F(M)WW = FWW = FW > FWWAAA > W
   = FWWGGG. Crop rotations with organic amendments were generally as
   profitable as rotations without amendments, though the price of hay and
   wheat determined which rotation was the most profitable.}},
