@article{ ISI:000279964600008,
Author = {Nahidan, Safoura and Nourbakhsh, Farshid},
Title = {{Microbial Indices Related to Soil Carbon as Affected by Management
   Practices in Arid Forest and Agricultural Ecosystems}},
