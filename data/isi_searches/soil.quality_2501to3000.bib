@article{ ISI:000319178700005,
Author = {Dradrach, Agnieszka and Karczewska, Anna},
Title = {{MERCURY IN SOILS OF MUNICIPAL LAWNS IN WROCLAW, POLAND}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2013}},
Volume = {{22}},
Number = {{4, SI}},
Pages = {{968-972}},
Abstract = {{This paper examines Hg concentrations in the surface soil layers (0-5 cm
   and 5-15 cm) of urban lawns, in three separate areas: east (E), south
   (S) and west (W) of the city of Wroclaw. Distinguished were three groups
   of lawns, with different functions: residential, located within housing
   estates (Res), roadside lawns, adjacent to the streets (St), and park
   lawns, situated in the inner parts of old parks (Pk). The concentrations
   of Hg in soil samples were in the range: 0.046-1.144 mg.kg(-1), and did
   not exceed soil quality standards established for urban soils (B
   category) at 2.0 mg.kg(-1). Statistically significant differences in
   soil Hg concentrations were found between the lawns, as related to their
   functions: the highest concentrations were present in soils of the park
   lawns, and the lowest - in the roadside lawns. This fact indicates the
   crucial importance of historical load of Hg as the main source of this
   element in soils of urban lawns in Wroclaw. Hg and Pb concentrations in
   0-5 cm layer were strongly correlated with each other. There were no
   significant differences in Hg concentrations in soils between three
   areas examined, situated in various parts of the city.}},
