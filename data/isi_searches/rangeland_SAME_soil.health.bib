@inproceedings{ ISI:000390822400011,
Author = {Wills, Skye and Williams, Candiss and Seybold, C. and Scheffe, Linda and
   Libohova, Zamir and Hoover, David and Talbot, Curtis and Brown, Joel},
Editor = {{Field, DJ and Morgan, CLS and McBratney, AB}},
Title = {{Using Soil Survey to Assess and Predict Soil Condition and Change}},
