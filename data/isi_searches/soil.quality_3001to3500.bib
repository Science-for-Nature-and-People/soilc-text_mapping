@article{ ISI:000310295700013,
Author = {de Carvalho, Fernanda and de Souza Moreira, Fatima Maria and Bran
   Nogueira Cardoso, Elke Jurandy},
Title = {{CHEMICAL AND BIOCHEMICAL PROPERTIES OF ARAUCARIA ANGUSTIFOLIA (BERT.)
   KTZE. FOREST SOILS IN THE STATE OF SAO PAULO}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
Year = {{2012}},
