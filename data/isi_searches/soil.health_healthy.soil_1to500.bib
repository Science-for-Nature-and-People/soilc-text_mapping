@article{ ISI:000284387600011,
Author = {Melakeberhan, Haddish},
Title = {{Assessing Cross-disciplinary Efficiency of Soil Amendments for
   Agro-biologically, Economically, and Ecologically Integrated Soil Health
   Management}},
