@inproceedings{ ISI:000261018500104,
Author = {Gao, Yang and Zhou, Pei and Miao, Chiyuan and Shi, Wanjun and Zhi, Yueer},
Editor = {{Li, JL}},
Title = {{Assessment of Heavy Metal Pollution of Industrial Soils in Shanghai by
   Multivariate Analysis, China}},
Booktitle = {{PROCEEDINGS OF 2008 INTERNATIONAL CONFERENCE ON RISK AND RELIABILITY
   MANAGEMENT, VOLS I AND II}},
