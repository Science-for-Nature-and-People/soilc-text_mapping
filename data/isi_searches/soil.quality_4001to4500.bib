@article{ ISI:000279225000014,
Author = {Reverchon, Frederique and del Pilar Ortega-Larrocea, Maria and
   Perez-Moreno, Jesus and Manuel Pena-Ramirez, Victor and Siebe, Christina},
Title = {{Changes in community structure of ectomycorrhizal fungi associated with
   Pinus montezumae across a volcanic soil chronosequence at Sierra
   Chichinautzin, Mexico}},
Journal = {{CANADIAN JOURNAL OF FOREST RESEARCH-REVUE CANADIENNE DE RECHERCHE
   FORESTIERE}},
Year = {{2010}},
Volume = {{40}},
