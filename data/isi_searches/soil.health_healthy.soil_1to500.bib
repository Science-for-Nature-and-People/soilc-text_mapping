@article{ ISI:000077955400016,
Author = {Rahman, MA and Gaffer, MA and Hossain, SMA and Tareque, AMM},
Title = {{Nitrogen balance sheet under forage cropping system in the old
   Brahmaputra floodplain soils}},
Journal = {{INDIAN JOURNAL OF AGRONOMY}},
Year = {{1998}},
Volume = {{43}},
Number = {{2}},
Pages = {{269-272}},
Month = {{JUN}},
Abstract = {{The balance sheet of nitrogen in the silt-loam alluvial soil was worked
   out from forage cropping systems under partially irrigated condition at
   Jamalpur, Bangladesh. Loss in N (4.3 to 470.3 kg/ha) was found in first
   year in all the systems. Gain in N was observed in the following year.
   Grasspea (Lathyrus sativus L.)-included system showed loss of N.
   Nitrogen loss was reduced remarkably with the inclusion of forage crops
   in the post-monsoon wet to dry transition period. Addition of forage
   crops further improved the apparent N recovery up to 58.6\% after 6
   intensive cropping seasons. Forage crops grown between lag periods were
   found beneficial in improving soil health and higher apparent recovery
   of applied N.}},
