@article{ ISI:000264218900030,
Author = {Vavoulidou, Evangelia and Avramides, Elisabeth and Wood, Martin and
   Lolos, Polykarpos},
Title = {{Response of Soil Quality Indicators to the Pesticide Cadusaphos}},
