@article{ ISI:000273736100003,
Author = {Mocali, S. and Dentice, A. and Marcucci, A. and Benedetti, A.},
Title = {{The impact of post-harvest treatments of transgenic eggplant residues on
   soil quality and microbial diversity}},
Journal = {{AGROCHIMICA}},
Year = {{2009}},
Volume = {{53}},
Number = {{5}},
Pages = {{296-307}},
Month = {{SEP-OCT}},
Abstract = {{A lot of literature concerning the impacts of genetically modified (GM)
   plants to the environment is available but actually there is the need to
   assess the impact of post-harvest treatments of transgenic plant
   residues on soil quality and microbial communities, as requested by
   European Directive 2001/18.
   In this study a Multidisciplinary approach was used to assess the
   effects of GM eggplants on soil quality and microbial diversity after
   two different treatments: cutting up and extirpation. Classical
   biochemical, ecophysiological (BIOLOG) and molecular methods (DGGE) were
   used. Samplings were performed just before treatments (T1), with plants
   still in the field, after 6 months (T2) and 12 months (T3). A
   significant difference in microbial respiration and diversity among GM
   plots and controls was observed in T1 but after 6 and 12 months Such
   differences disappeared, suggesting a strong correlation between plants
   and microorganisms, as well as a short-term impact.}},
