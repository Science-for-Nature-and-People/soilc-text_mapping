@article{ ISI:000366668000008,
Author = {Yang, Dejun and Bian, Zhengfu and Zhang, Kefeng and Xiong, Jibing and
   Lei, Shaogang},
Title = {{Quantitative Assessment of Soil Physical Quality in Northern China Based
   on S-theory}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
