@article{ ISI:000237051600007,
Author = {Green, VS and Stott, DE and Diack, M},
Title = {{Assay for fluorescein diacetate hydrolytic activity: Optimization for
   soil samples}},
Journal = {{SOIL BIOLOGY \& BIOCHEMISTRY}},
Year = {{2006}},
Volume = {{38}},
Number = {{4}},
Pages = {{693-701}},
Month = {{APR}},
Abstract = {{With the increased interest in integrated soil bioecosystem studies,
   there is a need to have a method of measuring overall microbial activity
   potential. Hydrolysis of fluorescein diacetate
   {[}3',6'-diacetylfluorescein (FDA)] has been suggested as a possible
   method because the ubiquitous lipase, protease, and esterase enzymes are
   involved in the hydrolysis of FDA. Following hydrolysis of FDA,
   fluorescein is released and can be measured spectrophotometrically. Our
   objective was to optimize the assay for FDA hydrolytic activity in soil
   samples and determine the kinetic parameters involved in this reaction.
   The optimized method involves extraction and quantitative measurement of
   the fluorescein released when 1.0 g of soil is incubated with 50 ml of
   60 mM Na-phosphate solution (buffered at pH 7.6) at 37 degrees C for 3
   h. Results showed that FDA hydrolysis was optimum at buffer pH 7.6 and
   the soil enzymes were denatured at temperatures above 60 degrees C.
   Three soils were used to optimize this method: Heiden clay, Raub silt
   loam, and Cecil sandy loam. This procedure is simple, precise, and can
   be used in commercial soil testing laboratories to determine general
   microbial activity and as a soil quality indicator. Published by
   Elsevier Ltd.}},
