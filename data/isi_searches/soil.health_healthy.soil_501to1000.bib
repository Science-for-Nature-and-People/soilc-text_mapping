@inproceedings{ ISI:000395217400023,
Author = {Meng Sida and Li Tianlai and Shang Miao and Zhang Shuo},
Editor = {{Zhang, Y}},
Title = {{Changes in Physicochemical Properties and Biological Activities of
   Facilities Continuous Tomato Cropping Soil}},
Booktitle = {{3RD CONFERENCE ON KEY TECHNOLOGY OF HORTICULTURE, CKTH 2011}},
Year = {{2011}},
Pages = {{99-102}},
Note = {{3rd Conference on Key Technology of Horticulture (CKTH 2011), Shenyang,
   PEOPLES R CHINA, DEC 17-18, 2011}},
Organization = {{Chinese Soc Horticultural Sci; Shenyang Agr Univ}},
Abstract = {{Researches on different batches of facilities continuous tomato cropping
   were conducted to compare the changes in physicochemical properties and
   biological activities, aiming at providing scientific and theoretical
   basis for selecting appropriate biology indexes for the soil health.
   Field experiments were done in plastic canopy, researching on changes in
   physicochemical properties, microbial community and soil enzymes
   activities of facilities continuous tomato cropping soil. With the
   batches of facilities continuous tomato cropping increasing, pH of soil
   decreased; the content of EC, organic matter, total nitrogen, total
   phosphorus, ammonium nitrogen, nitrate nitrogen and available phosphorus
   increased; The content of available potassium first decreased and then
   increased; the number of soil bacteria decreased, but fungus and
   actinomyces increased; the activity of soil hydrogenperoxidase and
   protease had a trend of decreased firstly and then increased; the soil
   urease activity increased firstly and then decreased; the soil
   polyphenol oxidase and invertase activity decreased. Facilities
   continuous tomato cropping will cause the imbalance of soil nutrients,
   which has a great influence on the quantity of the soil microbial
   community, and make the activites of different kinds of soil enzymes
   have different trends to change.}},
