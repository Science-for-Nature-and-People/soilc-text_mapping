@inproceedings{ ISI:000257018700086,
Author = {Wang, J. H. and Lu, X. G. and Jiang, M.},
Editor = {{Li, G and Jia, Z and Fu, Z}},
Title = {{Fuzzy synthetic evaluation of surface water quality of Naoli River on
   Sanjiang Plain, northeast China}},
