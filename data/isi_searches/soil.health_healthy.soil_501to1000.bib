@article{ ISI:000329556600019,
Author = {Kloeke, A. E. Elaine van Ommen and Gong, Ping and Ellers, Jacintha and
   Roelofs, Dick},
Title = {{EFFECTS OF A NATURAL TOXIN ON LIFE HISTORY AND GENE EXPRESSION OF
   EISENIA ANDREI}},
