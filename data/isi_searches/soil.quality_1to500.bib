@article{ ISI:000403236200031,
Author = {Guo, Zhibin and Liu, Hui and Wan, Shuixia and Hua, Keke and Jiang,
   Chaoqiang and Wang, Daozhong and He, Chuanlong and Guo, Xisheng},
Title = {{Enhanced yields and soil quality in a wheat-maize rotation using buried
   straw mulch}},
Journal = {{JOURNAL OF THE SCIENCE OF FOOD AND AGRICULTURE}},
Year = {{2017}},
Volume = {{97}},
Number = {{10}},
Pages = {{3333-3341}},
Month = {{AUG 15}},
Abstract = {{BACKGROUNDStraw return may improve soil quality and crop yields. In a
   2-year field study, a straw return method (ditch-buried straw return,
   DB-SR) was used to investigate the soil quality and crop productivity
   effects on a wheat-corn rotation system. This study consisted of three
   treatments, each with three replicates: (1) mineral fertilisation alone
   (CK0); (2) mineral fertilisation+7500kgha(-1) wheat straw incorporated
   at depth of 0-15cm (NPKWS); and (3) mineral fertilisation+7500kgha(-1)
   wheat straw ditch buried at 15-30cm (NPKDW).
   RESULTSNPKWS and NPKDW enhanced crop yield and improved soil biotical
   properties compared to mineral fertilisation alone. NPKDW contributed to
   greater crop yields and soil nutrient availability at 15-30cm depths,
   compared to NPKWS treatment. NPKDW enhanced soil microbial activity and
   bacteria species richness and diversity in the 0-15cm layer. NPKWS
   increased soil microbial biomass, bacteria species richness and
   diversity at 15-30cm.
   CONCLUSIONSThe comparison of the CK0 and NPKWS treatments indicates that
