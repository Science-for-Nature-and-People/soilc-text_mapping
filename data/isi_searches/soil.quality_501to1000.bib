@article{ ISI:000374716400009,
Author = {Lee, Jae-Cheol and Kim, Eun Jung and Kim, Hyun-Woo and Baek, Kitae},
Title = {{Oxalate-based remediation of arsenic bound to amorphous Fe and Al
   hydrous oxides in soil}},
Journal = {{GEODERMA}},
Year = {{2016}},
