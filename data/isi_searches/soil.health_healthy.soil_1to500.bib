@article{ ISI:000276769800010,
Author = {Ranjard, Lionel and Dequiedt, Samuel and Jolivet, Claudy and Saby,
   Nicolas P. A. and Thioulouse, Jean and Harmand, Jerome and Loisel,
   Patrice and Rapaport, Alain and Fall, Saliou and Simonet, Pascal and
