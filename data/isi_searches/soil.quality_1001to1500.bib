@article{ ISI:000359314500010,
Author = {de Alencar, Thiago Leite and Chaves, Arilene Franklin and dos Santos,
   Carlos Levi Anastacio and de Assis Junior, Raimundo Nonato and Anunciato
   Mota, Jaedson Claudio},
Title = {{PHYSICAL PROPERTIES OF A CULTIVATED CAMBISOL TREATED WITH BIOFERTILIZER
   IN THE APODI PLATEAU, CEARA, BRAZIL}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
Year = {{2015}},
