@article{ ISI:000248237900006,
Author = {Gelder, B. K. and Cruse, R. M. and Zhang, X. Y.},
Title = {{Comparison of track and tire effects of planter tractors on corn yield
   and soil properties}},
Journal = {{TRANSACTIONS OF THE ASABE}},
Year = {{2007}},
Volume = {{50}},
Number = {{2}},
Pages = {{365-370}},
Month = {{MAR-APR}},
Abstract = {{Soil compaction remains a serious concern throughout crop production
   areas of the world because of its deleterious effects on crop yield and
   soil quality. Many studies indicate significant yield losses due to
   compaction, others indicate significant differences in yield loss
   between tires and track,yet others show no differences at all for
   reasons that have not been fully explained. To investigate tractive
   method effects on corn (Zea mays L.) yield, Clarion-Nicollet-Webster
   soil was compacted with four-wheel drive and mechanical front-wheel
   drive class tractors equipped with both tires and tracks. The planter
   centerline was offset from the tractor drawbar to obtain plant data from
   crop rows with a range of distance values from the tire centerline.
   Plant emergence, grain yield, and penetrometer resistance were analyzed
   to determine compaction treatment and distance effects, and no
   significant treatment differences were found. A method for estimating
   critical soil stress levels required to cause structural degradation was
   also implemented; these results similarly suggest that treatment
   differences should not be observed.}},
