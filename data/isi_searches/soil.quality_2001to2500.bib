@article{ ISI:000331430100005,
Author = {Devi, T. Indira and Yadava, P. S. and Garkoti, S. C.},
Title = {{Cattle grazing influences soil microbial biomass in sub-tropical
   grassland ecosystems at Nambol, Manipur, northeast India}},
Journal = {{TROPICAL ECOLOGY}},
Year = {{2014}},
Volume = {{55}},
Number = {{2}},
Pages = {{195-206}},
Month = {{MAY}},
Abstract = {{We assessed the effects of grazing on the soil microbial biomass C, N
   and P in the soils of non-grazed (NG), moderately grazed (MG) and
   heavily grazed (HG) sub-tropical grasslands in the Imphal valley of
   Manipur, northeast India, from January to December 2007. Mean soil
   microbial C (mu g C g(-1) soil) ranged from 258.5 to 347.8 in 0 - 10 cm
   soil depth and 87.6 to 146.1 in 10 - 20 cm soil depth across all sites.
   Soil microbial N (mu g N g(-1) soil) fluctuated between 38.3 to 46.4
   soil in 0 - 10 cm soil depth and 11.7 to 16.2 in 10 - 20 cm soil depth
   across all sites. Mean soil microbial P (mu g P g(-1) soil) varied from
   19.0 to 22.6 in 0 - 10 cm soil depth and from 5.8 to 7.6 in 10 - 20 cm
   soil depth across all sites. The soil microbial biomass C, N and P were
   highest in the MG site, followed by the NG and HG sites. Soil microbial
   biomass was strongly influenced by season and by soil organic C, total N
   and total P in the grassland soils, as evidenced by positive,
   significant correlations with these soil properties. Our results
   indicate that moderate grazing enhances soil microbial biomass C, N and
   P. Moderate grazing may benefit sub-tropical grasslands in northeast
   India by influencing nutrient dynamics and could be prescribed for the
   management of these grasslands.}},
