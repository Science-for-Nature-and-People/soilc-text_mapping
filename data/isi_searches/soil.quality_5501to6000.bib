@article{ ISI:000244331800003,
Author = {Zhao, X. and Lu, J. and Wang, G.},
Title = {{Soil nutrients and earthworm dynamics in a bamboo-chicken agroforestry
   system in southern China}},
Journal = {{AGROCHIMICA}},
Year = {{2006}},
Volume = {{50}},
Number = {{3-4}},
Pages = {{111-121}},
Month = {{MAY-AUG}},
Abstract = {{Bamboo-chicken agroforestry system is a new agroforestry pattern in the
   hilly region of Southern China, this pattern has high potential to be
   extended in China. This study aims at identifying the impact of
   bamboo-chicken agroforestry system on dynamics of soil quality. The
   experiment was implemented at a bamboo-chicken agroforestry farm in
   Zhejiang province in the Southern part of China, at the same farm one
   field with only bamboo forest was selected as check field. Monitoring
   indicators of soil quality were: organic matter (OM), total nitrogen
   JN), available nitrogen (AN), total phosphorus (TP), available
   phosphorus (AP), total potassium (AK), available potassium (AK) and
   earthworm (both quantity and mass).
   The study results showed that: 1) all nutrients and earthworm indicators
   had dynamic changes among seasons during a whole year, 2) soil nutrients
   of bamboo-chicken system were improved, AP was significant different in
   the two treatments (P < 0.01) and TP was also significant (P < 0.05), 3)
   soil organism indicators were more sensitive than the chemical ones,
   earthworm quantity and earthworm mass between bamboo-chicken system and
   CK were significant (P < 0.05), 4) all the rest variances were not
   significant. Phosphorus was a special nutrient in this agroforestry
   system.}},
