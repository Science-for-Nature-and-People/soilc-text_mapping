@article{ ISI:000240678400005,
Author = {Nunez-Regueira, Lisardo and Proupin-Castineiras, J. and Rodriguez-Anon,
   J. A. and Villanueva-Lopez, Maria and Nunez-Fernandez, O.},
Title = {{Design of an experimental procedure to assess soil health state}},
