@article{ ISI:000188540600015,
Author = {Fu, BJ and Liu, SL and Chen, LD and Lu, YH and Qiu, J},
Title = {{Soil quality regime in relation to land cover and slope position across
   a highly modified slope landscape}},
Journal = {{ECOLOGICAL RESEARCH}},
Year = {{2004}},
Volume = {{19}},
