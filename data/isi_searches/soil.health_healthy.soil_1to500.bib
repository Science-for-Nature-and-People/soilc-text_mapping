@article{ ISI:000224671000001,
Author = {Jat, ML and Pal, SS and Shukla, L and Mathur, JMS and Singh, M},
Title = {{Rice (Oryza sativa) residue management using cellulolytic fungi and its
   effect on wheat (Triticum aestivum) yield and soil health in rice-wheat
   cropping system}},
Journal = {{INDIAN JOURNAL OF AGRICULTURAL SCIENCES}},
Year = {{2004}},
Volume = {{74}},
Number = {{3}},
Pages = {{117-120}},
Month = {{MAR}},
Abstract = {{A field investigation was carried out during 1999-2000 and 2000-2001 on
   a sandy-loam soil at Modipuram, Uttar Pradesh, to evolve efficient
   management practices for rice (Oryza sativa L.) residue recycling in the
   succeeding wheat (Triticum aestivum L. emend. Fiori \& Paol.) crop. Five
   rice residue-management practices comprising residue incorporated (T-1),
   residue burnt in-situ (T-2), residue incorporated + mixed culture of
   Trichoderma reesei Simmons, T. viridi Pers. and Aspergillus awamori
   Nakaz. (T-3), residue incorporated + N-enriched phosphocompost (T-4) and
   residue burnt + N-enriched phosphocompost (T-5) were allocated in
   randomized block design with 4 replications. In-situ burning of rice
   residue + N-enriched phosphocompost though found on a par with residue
   incorporation + N-enriched phosphocompost, resulted in a maximum grain
   yield; being 48.8 and 44.6\% higher than mere incorporation of rice
   residue during the both years. The treatment effects on soil fertility
   in succeeding wheat behaved alike the grain yield. The total soil
   microbial count (Azotobacter, fungi, bacteria and actinomycetes) 30 and
   60 days after sowing revealed that microfloral population was higher
   under all the residue management practice compared to in-situ burning.
   Addition of phosphocompost harboured more Azotobacter population,
   whereas bacterial population was higher under rice residue incorporation
   and inoculated with cellulolytic fungi.}},
