@article{ ISI:000234908500003,
Author = {Lilley, AK and Bailey, MJ and Cartwright, C and Turner, SL and Hirsch,
   PR},
Title = {{Life in earth: the impact of GM plants on soil ecology?}},
Journal = {{TRENDS IN BIOTECHNOLOGY}},
Year = {{2006}},
Volume = {{24}},
Number = {{1}},
Pages = {{9-14}},
Month = {{JAN}},
Abstract = {{The impact of changes incurred by agricultural biotechnology has led to
   concern regarding soil ecosystems and, rightly or wrongly, this has
   focused on the introduction of genetically modified (GM) crops. Soils
   are key resources, with essential roles in supporting ecosystems and
   maintaining environmental quality and productivity. The complexity of
   soils presents difficulties to their inclusion in the risk assessment
   process conducted for all GM plants. However, a combined approach,
   informed by both soil ecology and soil quality perspectives, that
   considers the impacts of GM crops in the context of conventional
   agricultural practices can provide a regulatory framework to ensure the
   protection of soils without being overly restrictive.}},
