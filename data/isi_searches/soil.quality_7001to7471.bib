@article{ ISI:A1997YJ48500008,
Author = {Yeates, GW and Saggar, S and Daly, BK},
Title = {{Soil microbial C, N, and P, and microfaunal populations under Pinus
   radiata and grazed pasture land-use systems}},
Journal = {{PEDOBIOLOGIA}},
Year = {{1997}},
Volume = {{41}},
Number = {{6}},
Pages = {{549-565}},
Month = {{NOV}},
Abstract = {{Conversion of improved pastures to Pinus radiata plantations is a
   significant land-use change and provides an opportunity to evaluate the
   effects of changes in chemical and biological soil parameters on soil
   organic matter dynamics. We assessed changes in soil microbial and
   microfaunal populations for a series of paired sites in the Manawatu
   region, New Zealand. The change in land-use to P. radiata was associated
   with declines in soil pH, microbial C and N, soil C and N, soil C : N
   ratio, microbial C : organic C ratio, total nematodes, most nematode
   functional groups, and nematode diversity in 0-10 cm mineral soil. These
   shifts reflect a decline in both the quantity and quality of organic
   matter input to mineral soil, with lower \% organic C contents and wider
   C : N ratio in soils under P. radiata. While these declines could be
   regarded as reductions in `soil quality', such changes involve shifts in
   soil properties originating from pasture rather than from the original,
   native vegetation. As the soil microbial biomass and soil microfauna
   readily recolonize depopulated areas once the trees are harvested, we
   see few problems in restoring the populations of soil organisms
   important in controlling nutrient cycling to sites of small woodlots
   after felling and return to pasture.}},
