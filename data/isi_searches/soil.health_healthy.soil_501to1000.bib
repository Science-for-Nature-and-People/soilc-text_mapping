@inproceedings{ ISI:000323621100006,
Author = {de Boer, M.},
Editor = {{VanDenEnde, JE and Krikke, AT and DenNijs, APM}},
Title = {{Producing Bulbs and Perennials; Sustainable Control of Diseases, Pests
   and Weeds}},
Booktitle = {{X INTERNATIONAL SYMPOSIUM ON FLOWER BULBS AND HERBACEOUS PERENNIALS}},
Series = {{Acta Horticulturae}},
