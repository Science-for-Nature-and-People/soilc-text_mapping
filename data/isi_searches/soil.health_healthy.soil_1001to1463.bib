@article{ ISI:000402547000004,
Author = {Khati, Priyanka and Chaudhary, Parul and Gangola, Saurabh and Bhatt,
   Pankaj and Sharma, Anita},
Title = {{Nanochitosan supports growth of Zea mays and also maintains soil health
   following growth}},
Journal = {{3 BIOTECH}},
Year = {{2017}},
Volume = {{7}},
Month = {{MAY 12}},
Abstract = {{The present study evaluated the effect of nanochitosan in combination
   with plant growth promoting rhizobacteria (PGPR), PS2 and PS10 on maize
   growth. The PGPR were earlier recognized as Bacillus spp. on the basis
   of 16S rDNA sequencing. The observation revealed enhanced plant health
   parameters like seed germination (from 60 to 96.97\%), plant height
   (1.5-fold increase), and leaf area (twofold). Variability in different
   physicochemical parameters (pH, oxidizable organic carbon, available
   phosphorous, available potassium, ammoniacal nitrogen and nitrate
   nitrogen) was observed. Activities of soil health indicator enzymes
   (dehydrogenase, fluorescein diacetate hydrolysis and alkaline
   phosphatase) were also enhanced 2 to 3 fold. Plant metabolites with
   respect to different treatments were also analyzed using gas
   chromatography-mass spectroscopy (GC-MS) and the result revealed an
   increase in the amounts of alcohols, acid ester and aldehyde compounds.
   Increase in organic acids indicates increased stress tolerance mechanism
   operating in maize plant after treatment of nanochitosan.}},
