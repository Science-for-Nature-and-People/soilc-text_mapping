@article{ ISI:000399241500038,
Author = {Badescu, Iulia Simona and Bulgariu, Dumitru and Bulgariu, Laura},
Title = {{Alternative utilization of algal biomass (Ulva sp.) loaded with Zn(II)
   ions for improving of soil quality}},
Journal = {{JOURNAL OF APPLIED PHYCOLOGY}},
