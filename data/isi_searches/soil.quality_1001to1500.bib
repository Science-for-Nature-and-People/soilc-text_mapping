@incollection{ ISI:000384347200016,
Author = {Peltz, Christopher D. and Harley, Andrew},
Editor = {{Guo, M and He, G and Uchimiya, SM}},
Title = {{Biochar Application for Abandoned Mine Land Reclamation}},
Booktitle = {{AGRICULTURAL AND ENVIRONMENTAL APPLICATIONS OF BIOCHAR: ADVANCES AND
   BARRIERS}},
