@article{ ISI:000398759800033,
Author = {Narendrula-Kotha, Ramya and Nkongolo, Kabwe K.},
Title = {{Changes in enzymatic activities in metal contaminated and reclaimed
   lands in Northern Ontario (Canada)}},
Journal = {{ECOTOXICOLOGY AND ENVIRONMENTAL SAFETY}},
