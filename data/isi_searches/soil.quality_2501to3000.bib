@article{ ISI:000327014400007,
Author = {Thomazini, Andre and Almeida de Azevedo, Humberto Carlos and Pinheiro,
   Patrick Leal and Mendonca, Eduardo de Sa},
Title = {{PARTICIPATORY INDICATORS OF QUALITY IN CONILON COFFEE AND SOIL IN
   AGROFORESTRY SYSTEM AND CONVENTIONAL}},
Journal = {{BIOSCIENCE JOURNAL}},
Year = {{2013}},
Volume = {{29}},
Number = {{5, 1}},
Pages = {{1469-1478}},
Month = {{NOV}},
Abstract = {{Participatory evaluation of agroecosystems is a fast and easy
   application for assessing the sustainability of family farming
   properties. This study aimed to implement in five farms in Alegre (ES),
   a practical and easily applied by farmers in participatory evaluation of
   quality characteristics Coffea canephora and soil. Five sites were
   evaluated with conventional management system agroecological farming and
   conservation. From observations made in the field notes were assigned to
   the indicators of quality of coffee and soil. The scores range from 1 to
   10 in 3 steps (0-5, 5-8 and 8-10). Graphics in the form of radar were
   plotted, representing the state of each indicator and being compared
   with the different perceptions of producers and technicians of the
   agricultural site. The diagram indicates the quality of coffee in the
   conventional high dependence on external inputs (note 2) and large
   effect of solar radiation by the lack of vegetation in dry hands (note
   3). As for the soil quality in conservation system showed better erosion
   control (note 9), increased depth of the dark layer (note 9) and more
   diversity of plants (note 9.5). Agroecological farming systems
   conservationists promote greater sustainability of family farming and
   optimize agricultural production.}},
