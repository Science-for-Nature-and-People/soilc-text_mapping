@article{ ISI:000351248500043,
Author = {Wu, Wei and Ma, Baoluo},
Title = {{Integrated nutrient management (INM) for sustaining crop productivity
   and reducing environmental impact: A review}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
