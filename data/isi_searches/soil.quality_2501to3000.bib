@inproceedings{ ISI:000366031900069,
Author = {Sandu, Mirela-Alina and Bica, Ioan and Virsta, Ana and Preda, Mihaela},
Book-Group-Author = {{SGEM}},
Title = {{HUMAN HEALTH RISK OF CONTAMINATION BY POLYCHLORINATED BIPHENYLS IN THE
   AREA OF BUCHAREST CITY}},
Booktitle = {{GEOCONFERENCE ON WATER RESOURCES, FOREST, MARINE AND OCEAN ECOSYSTEMS}},
Series = {{International Multidisciplinary Scientific GeoConference-SGEM}},
Year = {{2013}},
Pages = {{545-552}},
Note = {{13th International Multidisciplinary Scientific Geoconference, SGEM
   2013, Albena, BULGARIA, JUN 16-22, 2013}},
Abstract = {{This paper presents an original research regarding the risk for human
   health in the area of Bucharest city. Urban soil quality may be severely
   affected by PCBs contamination, impairing environmental and human
   health. Recent researches showed the incidence of polychlorinated
   biphenyls (PCBs) in the urban area of Bucharest. Due to the high level
   of toxicity, international legislation provides a list of PCBs compounds
   to be monitored: PCBs 28, 52, 101, 118, 138, 153, 180. Samples
   collection was made in 42 points located in gardens, parks, main streets
   and intersections. The chemical analyses were conducted in the
   Laboratory of National Research and Development Institute for Soil
   Science, Agro-Chemistry and Environment from Bucharest, according to an
   own analytical method adapted after EPA. According to the Romanian
   standards, the concentrations of PCBs overcome the normal values in the
   most sampling points. In a few points - located mainly in parks and
   vegetable gardens - the concentration overcomes the alert threshold.
   This could have a high impact on people health especially children and
   old people.
   The human health risk assessment was modeled using SADA software -
   Spatial Analysis and Decision Assistance. The SADA model follows the
   EPA's Risk Assessment Guidance and determines the risk of adverse health
   impacts of the population exposed to the chemicals found in soil. SADA
   calculates the risk, screening the values identified of contaminants
   found in the areas of concern. The results are integrated into modules
   as risk map, tabular screening for identified areas and spatial point of
   risk calculation.}},
