@inproceedings{ ISI:000228276600025,
Author = {Han, J and Sheth, AR and Porbaha, A and Shen, SL},
Editor = {{Yegian, MK and Kavazanjian, E}},
Title = {{Numerical analysis of embankment stability over deep mixed foundations}},
Booktitle = {{GEOTECHNICAL ENGINEERING FOR TRANSPORTATION PROJECTS, VOL 2}},
Series = {{GEOTECHNICAL SPECIAL PUBLICATION}},
