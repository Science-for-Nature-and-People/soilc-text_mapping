@article{ ISI:000189189700005,
Author = {Egharevba, F and Olite, M and Osakwe, SA},
Title = {{Studies on the effect of laundry effluent on the physicochemical
   properties of soils in Edo state of Nigeria}},
Journal = {{DISCOVERY AND INNOVATION}},
Year = {{2003}},
Volume = {{15}},
Number = {{1-2}},
Pages = {{11-16}},
Month = {{JUN}},
Abstract = {{Laundry effluent has the potentials to affect soil physicochemical
   properties. In an effort to examine the effect of laundry effluent,
   topsoil from twelve sites in Edo State were treated with composite
   mixtures of liquid effluents obtained from six laundry industries and
   analyzed for their physicochemical properties. The treatment of the
   topsoil with laundry effluent resulted in significant increase (P=0.05)
   in pH, available phosphorus, exchangeable sodium, potassium,
   exchangeable sodium percentage, percentage base saturation, soil
   buffering capacity and a significant decrease (P=0.05) in calcium,
   hydrogen and cation exchange capacity. There was no significant
   difference between the untreated and treated soils with respect to the
   organic carbon, nitrogen, magnesium and the distribution of clay, silt
   and sand. Thus, the treatment with laundry effluent showed a tendency to
   reduce soil quality with respect to sodium, exchangeable sodium
   percentage, calcium and cation exchange capacity.}},
