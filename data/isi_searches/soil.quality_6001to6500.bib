@article{ ISI:000222912100002,
Author = {Delate, K and DeWitt, J},
Title = {{Building a farmer-centered land grant university organic agriculture
   program: A Midwestern partnership}},
Journal = {{RENEWABLE AGRICULTURE AND FOOD SYSTEMS}},
