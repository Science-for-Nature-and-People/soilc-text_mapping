Author = {Lippe, M. and Minh, T. Thai and Neef, A. and Hilger, T. and Hoffmann, V.
   and Lam, N. T. and Cadisch, G.},
Title = {{Building on qualitative datasets and participatory processes to simulate
   land use change in a mountain watershed of Northwest Vietnam}},
Journal = {{ENVIRONMENTAL MODELLING \& SOFTWARE}},
