@article{ ISI:000341476700008,
Author = {Pires, Luiz F. and Brinatti, Andre M. and Saab, Sergio C. and Cassaro,
   Fabio A. M.},
Title = {{Porosity distribution by computed tomography and its importance to
   characterize soil clod samples}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2014}},
Volume = {{92}},
Pages = {{37-45}},
Month = {{SEP}},
Abstract = {{Gamma-ray computed tomography (CT) was employed to study the soil
   quality of clod samples used to investigate porosity (phi). Samples with
   volumes varying from 50 to 100 cm(3) were collected from the soil
   surface. 2D CT images were obtained with millimetric resolution.
   Porosity distribution analyses were carried out to infer the soil clod
   structure. Results obtained provided a new insight on the variability of
   internal clod structure due to the large amount of data analyzed,
   information that is not provided by traditional methods used in physics
   applied to soil. (C) 2014 Elsevier Ltd. All rights reserved.}},
