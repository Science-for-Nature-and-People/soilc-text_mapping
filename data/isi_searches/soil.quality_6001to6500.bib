@inproceedings{ ISI:000239952800030,
Author = {Garcia-Gil, J. C. and Plaza, C. and Polo, A.},
Editor = {{Lekkas, TD}},
Title = {{Utilization of biological parameters as early indicators of soil
   restoration in a long-term field experiment with municipal waste compost}},
