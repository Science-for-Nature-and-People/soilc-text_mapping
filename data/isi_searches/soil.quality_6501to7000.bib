@article{ ISI:000082503400004,
Author = {Gray, LC},
Title = {{Is land being degraded? A multi-scale investigation of landscape change
   in southwestern Burkina Faso}},
Journal = {{LAND DEGRADATION \& DEVELOPMENT}},
