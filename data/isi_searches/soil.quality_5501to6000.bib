@article{ ISI:000230707200001,
Author = {Li, YT and Becquer, T and Quantin, C and Benedetti, M and Lavelle, P and
   Jun, D},
Title = {{Microbial activity indices: Sensitive soil quality indicators for trace
   metal stress}},
Journal = {{PEDOSPHERE}},
Year = {{2005}},
Volume = {{15}},
Number = {{4}},
Pages = {{409-416}},
Month = {{AUG}},
Abstract = {{Physicochemical properties, total and DTPA
   (diethylenetriaminepentaacetic acid)-extractable Cu, Zn, Pb and Cd
   contents, microbial biomass carbon (C) content and the organic C
   mineralization rate of the soils in a long-term trace metal-contaminated
   paddy region of Guangdong, China were determined to assess the
   sensitivity of microbial indices to moderately metal-contaminated paddy
   soils. The mean contents of total Cu, Zn, Pb and Cd were 251, 250, 171,
   and 2.4 mg kg(-1) respectively. DTPA-extractable metals were correlated
   positively and significantly with total metals, CEC, and organic C
   (except for DTPA-extractable Cd), while they were negatively and highly
   significantly correlated with pH, total Fe and Mn. Metal stress resulted
   in relatively low ratios of microbial biomass C to organic C and in
   remarkable inhibition of the microbial metabolic quotient and C
   mineralization rate, which eventually led to increases in soil organic C
   and C/N. Moreover, microbial respiratory activity showed a stronger
   correlation to DTPA-extractable metals than to total metal content.
   Likewise, in the acid paddy soils some ``linked{''} microbial activity
   indices, such as metabolic quotient and ratios of basal respiration to
   organic C, especially during initial incubation, were found to be more
   sensitive indicators of soil trace metal contamination than microbial
   biomass C or basal respiration alone.}},
Publisher = {{SCIENCE PRESS}},
Address = {{16 DONGHUANGCHENGGEN NORTH ST, BEIJING 100717, PEOPLES R CHINA}},
