@article{ ISI:000242489300006,
Author = {Oliver, M. A. and Archer, J. R. and Baxter, S. J. and Todd, A. D. and
   Skinner, R. J.},
Title = {{The Representative Soil Sampling Scheme of England and Wales: a
   statistical analysis of topsoil nutrient status and pH between 1971 and
   2001}},
