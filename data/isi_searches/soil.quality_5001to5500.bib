@article{ ISI:000252404300008,
Author = {Ros, Margarita and Garcia, Carlos and Hernandez, Maria Teresa},
Title = {{Evaluation of different pig slurry composts as fertilizer of
   horticultural crops: Effects on selected chemical and microbial
   properties}},
Journal = {{RENEWABLE AGRICULTURE AND FOOD SYSTEMS}},
