@article{ ISI:000284714200010,
Author = {Fernando, Ana L. and Duarte, Maria P. and Almeida, Joana and Boleo, Sara
   and Mendes, Benilde},
Title = {{Environmental impact assessment of energy crops cultivation in Europe}},
Journal = {{BIOFUELS BIOPRODUCTS \& BIOREFINING-BIOFPR}},
