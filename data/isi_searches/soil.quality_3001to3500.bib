@article{ ISI:000312350800009,
Author = {Komnitsas, Kostas and Zaharaki, Dimitra},
Title = {{Pre-treatment of olive mill wastewaters at laboratory and mill scale and
   subsequent use in agriculture: Legislative framework and proposed soil
   quality indicators}},
