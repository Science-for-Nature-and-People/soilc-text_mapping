@article{ ISI:000394079000002,
Author = {Mouas-Bourbia, S. and Yahiaoui, M. and Kiouas, H. and Issaoun, D. and
   Derridj, A.},
Title = {{Nitrogen bioavailability in the rhizosphere of Olea europea L. groves
   under arid climate}},
Journal = {{AGROCHIMICA}},
Year = {{2016}},
Volume = {{60}},
Number = {{3}},
Pages = {{173-185}},
Month = {{JUL-SEP}},
Abstract = {{The aim of this study was to evaluate the effect of the root of Olen
   europea L. grown under arid climate on bioavailability of soil nitrogen.
   Rhizospheric, rhizoplane and bulk soils from three olive groves
   cultivated under arid climate were sampled. Soil organic carbon and CEC
   of rhizoplane were significantly (p <= 50.05) greater than that of
   rhizospheric and bulk soils. The rhizoplane of two olive groves was
   significantly (p <= 50.05) enriched in N-NO3- and N-NH4+ + N-NO3-
   content. Despite low levels of organic matter, soils were well supplied
   with available nitrogen. In the rhizosphere, organic matter limited the
   increase of the electrical conductivity and improved soil CEC. The
   N-NH4+ and N-NO3- contents of rhizosphere were significantly and
   positively correlated to the soil organic carbon of the rhizosphere.
   These results suggest that the activity of the root of olive tree
   induces an improvement of soil quality at the soil-root interface.}},
