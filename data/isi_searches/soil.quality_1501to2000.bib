@article{ ISI:000345280700007,
Author = {Cheng, Qingli and Guo, Yujie and Wang, Wenlin and Hao, Shilong},
Title = {{Spatial variation of soil quality and pollution assessment of heavy
   metals in cultivated soils of Henan Province, China}},
Journal = {{CHEMICAL SPECIATION AND BIOAVAILABILITY}},
