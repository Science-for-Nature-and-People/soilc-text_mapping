@article{ ISI:000277383700006,
Author = {Guo, Tailong and Wang, Quanjiu and Li, Dingqiang and Wu, Laosheng},
Title = {{Sediment and solute transport on soil slope under simultaneous influence
   of rainfall impact and scouring flow}},
Journal = {{HYDROLOGICAL PROCESSES}},
