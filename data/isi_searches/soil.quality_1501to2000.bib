@article{ ISI:000350932700022,
Author = {Chandra, Avantika and Kumar, Vipin and Jain, Manish Kumar},
Title = {{THE SEASONAL CHANGES IN SOIL PROPERTIES DUE TO COAL MINE IMPACTS}},
Journal = {{CARPATHIAN JOURNAL OF EARTH AND ENVIRONMENTAL SCIENCES}},
