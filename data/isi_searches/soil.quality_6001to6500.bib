@article{ ISI:000188432900005,
Author = {Pandey, LM and Pal, S and Mruthyunjaya},
Title = {{Impact of zero-tillage technology in the rice (Oryza sativa)-wheat
   (Triticum aestivum) system of foothills of Uttaranchal State, India}},
Journal = {{INDIAN JOURNAL OF AGRICULTURAL SCIENCES}},
Year = {{2003}},
Volume = {{73}},
Number = {{8}},
Pages = {{432-437}},
Month = {{AUG}},
Abstract = {{A study was carried out during June 2002 to examine the impact of zero
   tillage in terms of changes in the cost structure, returns and
   resource-use efficiency in foothills region of Uttaranchal state of
   India, one of the major rice (Oryza sativa L.)-wheat(Triticum aestivum
   L. emend. Fiori \& Paol.) pockets of the Indo-Gangetic Plain. The
   parameters such as yield increase, reduction in cost of production and
   technical and economic efficiency were used to assess the economic
   benefits. Determinants of technical efficiency in terms of the adoption
   of zero-tillage technology and producer characteristics were also
   examined. Impressive cost savings (28\%) was observed under zero tillage
   in wheat. The technology had positive and significant impact on
   environment and sustainability of the system through improved soil
   quality and reduction in use of inputs like chemical fertilizers,
   irrigation water, and energy. The results showed that education of
   farmer was the most important factor in achieving resource-use
   efficiency, while size of holding was the most important influencing
   factor in adoption of zero-tillage technology. The gains from the
   adoption of zero-tillage technology could further be improved with
   enhanced efficiency in resource use and a wider adoption of the
   technology through appropriate extension programmes.}},
