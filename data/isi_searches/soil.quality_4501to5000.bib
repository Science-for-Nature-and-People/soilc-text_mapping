@article{ ISI:000274285100012,
Author = {Garcia-Teijeiro, Rosalia and Lightfoot, David A. and Hernandez, Jorge D.},
Title = {{Effect of a Chemical Modified Urea Fertilizer on Soil Quality: Soil
   Microbial Populations Around Corn Roots}},
Journal = {{COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS}},
