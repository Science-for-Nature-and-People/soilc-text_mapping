@article{ ISI:000235950700003,
Author = {Leon, MCC and Stone, A and Dickc, RR},
Title = {{Organic soil amendments: Impacts on snap bean common root rot
   (Aphanomyes euteiches) and soil quality}},
Journal = {{APPLIED SOIL ECOLOGY}},
Year = {{2006}},
Volume = {{31}},
