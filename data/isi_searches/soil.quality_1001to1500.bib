@article{ ISI:000363449400014,
Author = {Recha, C. W. and Mukopi, M. N. and Otieno, J. O.},
Title = {{Socio-Economic Determinants of Adoption of Rainwater Harvesting and
   Conservation Techniques in Semi-Arid Tharaka Sub-County, Kenya}},
Journal = {{LAND DEGRADATION \& DEVELOPMENT}},
