@article{ ISI:000234629100012,
Author = {Demsar, D and Dzeroski, S and Larsen, T and Struyf, J and Axelsen, J and
   Pedersen, MB and Krogh, PH},
Title = {{Using multi-objective classification to model communities of soil
   microarthropods}},
Journal = {{ECOLOGICAL MODELLING}},
Year = {{2006}},
Volume = {{191}},
Number = {{1, SI}},
Pages = {{131-143}},
Month = {{JAN 27}},
Note = {{4th International Workshop on Environmental Applications of Machine
   Learning (EAML), Bled, SLOVENIA, SEP 27-OCT 01, 2004}},
Abstract = {{In agricultural soil, a suite of anthropogenic events shape the
   ecosystem processes and populations. However, the impact from
   anthropogenic sources on the soil environment is almost exclusively
   assessed for chemicals, although other factors like crop and tillage
   practices have an important impact as well. Thus, the farming system as
   a whole should be evaluated and ranked according to its environmental
   benefits and impacts. Our starting point is a data set describing
   agricultural events and soil biological parameters. Using machine
   learning methods for inducing regression and model trees, we produce
   empirical models able to predict the soil quality from agricultural
   measures in terms of quantities describing the soil microarthropod
   community. We are also interested in discovering additional higher level
   knowledge. In particular, we have identified the most important factors
   influencing the population densities of springtails and mites and their
   biodiversity. We also identify to which agricultural actions different
   microarthropods react distinctly. To obtain this higher level knowledge,
   we employ multi-objective regression trees. (c) 2005 Elsevier B.V. All
   rights reserved.}},
