@inproceedings{ ISI:000235640900036,
Author = {Stocking, A and Obando, FH and Tengberg, A},
Editor = {{Cano, AF and Silla, RO and Mermut, AR}},
Title = {{Soil degradation and its impact on plants in drylands}},
Booktitle = {{SUSTAINABLE USE AND MANAGEMENT OF SOILS - ARID AND SEMIARID REGIONS}},
Series = {{ADVANCES IN GEOECOLOGY}},
Year = {{2005}},
Volume = {{36}},
Pages = {{355-368}},
Note = {{International Symposium on Sustainable Use and Management of Soils in
   Arid and Semiarid Regions, Tech Univ, Muralla del Mar Campus, Cartagena,
   SPAIN, SEP 22-26, 2002}},
Abstract = {{Soil degradation affects soil quality. This influences soil
   productivity, and in turn reduces plant production. The processes are
   complex and dynamic, involving changes In soil physical, chemical and
   biological properties, such as bulk density, soil organic matter and
   biodiversity. The food Security of the world's drylands is especially
   Susceptible to degradation processes that alter plant-available water in
   soils. Africa and Latin America both suffer widespread yield reductions
   from past and present erosion. Quantitative relationships between
   degradation and yield suggest a strongly curvilinear decline with some
   soils - Ferralsols and Acrisols - affected strongly, and others -
   Cambisols and Nitosols - being more resilient. A case study is described
   on Oxisols from the Eastern Plains of Colombia where water erosion has
   had a significant effect on soil physical properties, less effect on
   soil chemical properties, but a large impact on rice yields. Severe
   degradation was Measured as having lowered yields by 63\% as compared to
   slight degradation, which has occurred alongside a 51\% decline in soil
   organic matter and a 14\% increase in bulk density. Such knowledge of
   causal factors can assist soil managers in designing remedial measures,
   to maximise soil productivity recovery. In the Colombian case, practices
   that reduce soil erosion and increase soil organic matter, Such as
   minimum tillage, green manures and cover crops, are indicated. A better
   understanding of processes and dynamics of soil degradation on dryland
   soils will enhance our capability to meet the food security of poor but
   growing populations.}},
