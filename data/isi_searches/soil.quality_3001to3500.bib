@article{ ISI:000300884500002,
Author = {Loganathan, Paripurnanda and Vigneswaran, Saravanamuthu and Kandasamy,
   Jaya and Naidu, Ravi},
Title = {{Cadmium Sorption and Desorption in Soils: A Review}},
Journal = {{CRITICAL REVIEWS IN ENVIRONMENTAL SCIENCE AND TECHNOLOGY}},
Year = {{2012}},
Volume = {{42}},
Number = {{5}},
Pages = {{489-533}},
Abstract = {{Cadmium (Cd) is an environmental pollutant that can be readily taken up
   by plants and may enter the food chain, causing risk to human health. It
   can also affect soil quality and, if easily leached, has the potential
   to pollute surface and ground waters. Sorption and desorption are
   critical processes controlling the phytoavailability and mobility of Cd
   in soils. The authors present the present knowledge on the mechanisms
   and hysteresis of Cd sorption and desorption in soils and factors such
   as pH, ionic strength, index cation, other heavy metal cations,
   inorganic anions, organic ligands, Cd loading rate, and the type and
   amounts of organic matter and inorganic colloids influencing these
   processes. The methods used to determine the nature and magnitude of Cd
   sorption and desorption are also critically reviewed.}},
