@article{ ISI:000240034500010,
Author = {Ideriah, Tubonimi J. K. and Omuaru, Victor O. T. and Adiukwu, Patricia
   U.},
Title = {{Soil quality around a solid waste dumpsite in Port Harcourt, Nigeria}},
Journal = {{AFRICAN JOURNAL OF ECOLOGY}},
