@article{ ISI:000339726600012,
Author = {Anton, Jose M. and Tarquis, Ana M. and Grau, Juan B. and Sanchez, Elena
   and Saa, Antonio and Diaz, Mari-Cruz},
Title = {{Application of Decision Theory methods for a soil classification in the
   Community of Madrid (Spain)}},
Journal = {{ANNALS OF OPERATIONS RESEARCH}},
Year = {{2014}},
Volume = {{219}},
Number = {{1}},
Pages = {{203-229}},
Month = {{AUG}},
Abstract = {{A land classification method was designed for the Community of Madrid
   (CM), which has lands suitable for either agriculture use or natural
   spaces. The process started from an extensive previous CM study that
   contains sets of land attributes with data for 122 types and a
   minimum-requirements method providing a land quality classification (SQ)
   for each land. Borrowing some tools from Operations Research (OR) and
   from Decision Science, that SQ has been complemented by an additive
   valuation method that involves a more restricted set of 13
   representative attributes analysed using Attribute Valuation Functions
   to obtain a quality index, QI, and by an original composite method that
   uses a fuzzy set procedure to obtain a combined quality index, CQI, that
   contains relevant information from both the SQ and the QI methods.}},
