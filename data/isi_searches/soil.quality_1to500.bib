@article{ ISI:000385901200026,
Author = {Kheirfam, Hossein and Sadeghi, Seyed Hamidreza and Homaee, Mehdi and
   Darki, Behrouz Zarei},
Title = {{Quality improvement of an erosion-prone soil through microbial
   enrichment}},
Journal = {{SOIL \& TILLAGE RESEARCH}},
