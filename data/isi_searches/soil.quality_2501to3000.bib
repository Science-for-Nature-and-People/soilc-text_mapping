@inproceedings{ ISI:000345119700014,
Author = {Brookes, Philip C. and Pietri, Juan C. Aciego and Wu, Yuping and Xu,
   Jianming},
Editor = {{Xu, J and Sparks, DL}},
Title = {{Microbial Indicators of Soil Quality in Upland Soils}},
