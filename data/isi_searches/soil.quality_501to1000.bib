@article{ ISI:000370151700023,
Author = {Macci, Cristina and Doni, Serena and Peruzzi, Eleonora and Mennone,
   Carmelo and Masciandaro, Grazia},
Title = {{Biostimulation of Soil Microbial Activity Through Organic Fertilizer and
   Almond tree Association}},
Journal = {{LAND DEGRADATION \& DEVELOPMENT}},
