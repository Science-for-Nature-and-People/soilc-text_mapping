@article{ ISI:000080465300019,
Author = {Pinzari, F and Trinchera, A and Benedetti, A and Sequi, P},
Title = {{Defining soil quality in Mediterranean forest systems: Microbial biomass
   activity}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{1998}},
Volume = {{7}},
Number = {{5A-6A, SI}},
Pages = {{447-457}},
Month = {{NOV-DEC}},
Abstract = {{In natural, undisturbed ecosystems the meaning of soil quality should
   express the features of each system in its own pedoclimatic conditions,
   without referring quality to an universal standard of ``healthy{''}
   soil. Forest soil quality may be therefore defined as ``the soil ability
   to match the natural succession of the biocoenosis{''}. This study was
   conducted to test soil quality indicators based on microbial biomass
   activity nn Mediterranean forest soils. The influence of different plant
   communities and management practice on soil characteristics were
   investigated in eight sites on the Tyrrhenic coast near Rome. Metabolic
   quotient, C-CO2 production, microbial C to organic C ratio, C/N ratio,
   Cl estimate and qmC turned out powerful tools to depict both ecosystem
   dynamics and properties.}},
