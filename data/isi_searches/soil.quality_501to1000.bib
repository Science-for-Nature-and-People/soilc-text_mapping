Title = {{Effects of the integrated nutrient management on soil properties in
   table grape `Crimson Seedless' during establishment}},
Booktitle = {{III International Symposium on Organic Matter Management and Compost Use
   in Horticulture}},
Series = {{Acta Horticulturae}},
