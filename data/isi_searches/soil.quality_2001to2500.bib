@article{ ISI:000333022800012,
Author = {Liu, Zhanjun and Zhou, Wei and Shen, Jianbo and Li, Shutian and Ai, Chao},
Title = {{Soil quality assessment of yellow clayey paddy soils with different
   productivity}},
