@article{ ISI:000167138700013,
Author = {Preda, M and Popescu, A and Mateescu, M and Ilin, P},
Title = {{A study on the corrosion behaviour of the equipment made up of aluminium
   alloys, used for irigation purposes}},
Journal = {{REVISTA DE CHIMIE}},
Year = {{2000}},
Volume = {{51}},
Number = {{10}},
Pages = {{793-797}},
Month = {{OCT}},
Abstract = {{This study presents an analysis on the resistance to corrosion of some
   equipment (pipes) used in the irrigation systems. It is shown the
   influence of the anions and cations present in the watery environment
   which makes contact with the metal (aluminium or aluminium alloys) from
   which the irrigation pipes are made of. The study also shows some other
   influencing factors: dissolved O-2 in watery environment and soil
   quality. The experimental part of the study in the analysis of some
   irrigation pipes manufactured from aluminium alloys (the 3003 group)
   which have as main phase the intermetallical compounds Al6Mn. The
   metalographical studies are developed for the both transversal and
   longitudinal sections through the irrigation pipes that are under the
   influence of corrosive agents.}},
