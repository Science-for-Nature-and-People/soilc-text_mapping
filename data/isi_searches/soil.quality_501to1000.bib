@article{ ISI:000376282600006,
Author = {Ro, Kyoung S.},
Title = {{Kinetics and Energetics of Producing Animal Manure-Based Biochar}},
Journal = {{BIOENERGY RESEARCH}},
Year = {{2016}},
Volume = {{9}},
Number = {{2}},
Pages = {{447-453}},
Month = {{JUN}},
Abstract = {{Pyrolysis of animal manure produces biochar with multiple beneficial use
   potentials for improving soil quality and the environment. The kinetics
   and energetics of pyrolysis in producing manure-based biochar were
   reviewed and analyzed. Kinetic analysis of pyrolysis showed that the
   higher the temperature, the shorter the reaction time was needed for
   thermal decomposition and carbonization of animal manure. This kinetic
   information can assist in producing biochar with a desired proximate
   composition. Biochar with lower volatile matter (VM) content can be
   produced with either higher pyrolysis temperature or longer reaction
   time. Energetically, pyrolysis of wet manures is not sustainable due to
   high energy needed for drying moisture. However, co-pyrolysis with other
   high energy density wastes such as agricultural plastic wastes would
   produce not only energetically sustainable biochar but surplus energy as
