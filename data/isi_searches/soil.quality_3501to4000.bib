@article{ ISI:000301402900024,
Author = {de Brito Ferreira, Enderson Petronio and Wendland, Adriane and Didonet,
   Agostinho Dirceu},
Title = {{Microbial biomass and enzyme activity of a Cerrado Oxisol under
   agroecological production system}},
