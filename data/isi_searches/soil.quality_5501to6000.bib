@article{ ISI:000228704200014,
Author = {Baran, S and Olesuczuk, P and Milczarek, T},
Title = {{Application of the biochemical index of soil fertility to estimate the
   condition of sewage-sludge-amended soil and its dependence on the
   content of polycyclic aromatic hydrocarbons}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{2005}},
Volume = {{38}},
Number = {{4}},
Pages = {{429-436}},
Month = {{APR}},
Abstract = {{Application of sewage sludge into soil causes changes not only in its
   physical and chemical properties but also in its microbiological
   properties. One of the tests used to estimate the microbiological soil
   quality is based on the fertility coefficients of the enzymatic
   activity. The usefulness of the biochemical index of soil fertility
   (BISF) for evaluating the condition of microorganisms and its dependence
   on the contents of polycyclic aromatic hydrocarbons were evaluated in
   this research. Sewage sludge was introduced into soil at the following
   doses: 30, 75, 150, 300 and 600 t/ha. The content of polycyclic aromatic
   hydrocarbons (PAH) and the dependence between the PAH and the biological
   index of the soil fertility based on the enzymatic activity of
   dehydrogenases, phosphatases, ureases, and the total content of organic
   carbon were determined.}},
