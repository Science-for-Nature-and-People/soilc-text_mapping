@inproceedings{ ISI:000251800800019,
Author = {Staes, J. and Backx, H. and Meire, P.},
Editor = {{Moerlins, JE and Khankhasayev, MK and Leitman, SF and Makhmudov, EJ}},
Title = {{Integrated water management}},
Booktitle = {{TRANSBOUNDARY WATER RESOURCES: A FOUNDATION FOR REGIONAL STABILITY IN
   CENTRAL ASIA}},
