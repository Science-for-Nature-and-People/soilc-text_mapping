@article{ ISI:000266682200020,
Author = {Lafond, Guy and McConkey, B. G. and Stumborg, M.},
Title = {{Conservation tillage models for small-scale farming: Linking the
   Canadian experience to the small farms of Inner Mongolia Autonomous
   Region in China}},
Journal = {{SOIL \& TILLAGE RESEARCH}},
