@article{ ISI:000399624500040,
Author = {Yue, Yan and Lin, Qimei and Xu, Yunqing and Li, Guitong and Zhao,
   Xiaorong},
Title = {{Slow pyrolysis as a measure for rapidly treating cow manure and the
   biochar characteristics}},
Journal = {{JOURNAL OF ANALYTICAL AND APPLIED PYROLYSIS}},
Year = {{2017}},
Volume = {{124}},
Pages = {{355-361}},
Month = {{MAR}},
Abstract = {{It is imperative to develop an innovative method for quickly treating
   the large volume of cow manure produced in intensive cow farms in China,
   particularly in Inner Mongolia. In this study, cow manure was slowly
   pyrolyzed at different temperatures (300 degrees C, 400 degrees C, 500
   degrees C, and 700 degrees C for 2 h) and with retention times (0.5 h, 1
   h, and 2 h at 400 degrees C). The results showed that pH, electrical
   conductivity (EC), basic functional groups, and aromaticity enhanced,
   but both carboxyl functional groups and aliphaticity decreased when
   temperature increased from 300 degrees C to 500 degrees C and retention
   time increased from 0.5 to 1 hat 400 degrees C. However, most of these
   parameters of the collected biochars changed little when temperature
   increased from 500 degrees C to 700 degrees C and retention time
   increased from 1 h to 2 h at 400 degrees C, implying that the pyrolysis
   reached to relative stable status. These results suggested that the slow
   pyrolysis might be as a rapid measure for treating cow manure and
   producing biochar. The proper pyrolysis conditions could be 500 degrees
   C for 1-2 h, considering the relative high and stable characters of the
   harvested biochar. Further work is necessary to determine the impacts of
   the biochar on soil quality while used as a soil conditioner. (C) 2017
   Elsevier B.V. All rights reserved.}},
