@article{ ISI:000276655100011,
Author = {Zhang, Changshun and Xie, Gaodi and Fan, Shaohui and Zhen, Lin},
Title = {{Variation in Vegetation Structure and Soil Properties, and the Relation
   Between Understory Plants and Environmental Variables Under Different
   Phyllostachys pubescens Forests in Southeastern China}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
