@inproceedings{ ISI:000277196300045,
Author = {Sadeghi, Hossein and Bahrani, Mohammad Jafar},
Book-Group-Author = {{IEEE}},
Title = {{New approach to prevent burning crop residue by creating residue mulch
   (Air Pollution Control)}},
Booktitle = {{PROCEEDINGS OF THE SECOND INTERNATIONAL CONFERENCE ON ENVIRONMENTAL AND
