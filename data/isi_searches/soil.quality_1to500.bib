@article{ ISI:000401674400003,
Author = {Morris, Jeffrey and Brown, Sally and Cotton, Matthew and Matthews, H.
   Scott},
Title = {{Life-Cycle Assessment Harmonization and Soil Science Ranking Results on
   Food-Waste Management Methods}},
