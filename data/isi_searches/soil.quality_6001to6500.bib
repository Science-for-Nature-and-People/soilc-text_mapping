@article{ ISI:000177960200008,
Author = {Eve, MD and Sperow, M and Howerton, K and Paustian, K and Follett, RF},
Title = {{Predicted impact of management changes on soil carbon storage for each
   cropland region of the conterminous United States}},
Journal = {{JOURNAL OF SOIL AND WATER CONSERVATION}},
Year = {{2002}},
Volume = {{57}},
Number = {{4}},
Pages = {{196-204}},
Month = {{JUL-AUG}},
Abstract = {{The exchange of carbon dioxide between the atmosphere, biosphere and
   pedosphere is an important consideration when assessing agricultural and
   environmental management or policy decisions and their relationship to
   climate change. Field experimentation is the best way to gather data,
   but experimental data are specific to the management, soils and climate
   that represent the research site. Inadequate field data exist to address
   all management options across all soil types and climates. We have used
   the Intergovernmental Panel on Climate Change inventory approach to
   estimate changes in soil carbon storage resulting from various land use
   and management options. We generate a regional assessment of the
   relative impact of implementing changes in agricultural management on
   soil carbon storage. For each agricultural region of the United States,
   we present an estimated annual change in soil carbon storage for each
   management option. Results should prove especially useful in evaluating
   management options and tradeoffs.}},
