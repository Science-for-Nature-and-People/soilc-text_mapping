@inproceedings{ ISI:000380441700113,
Author = {Wang Hui-jing},
Book-Group-Author = {{IEEE}},
Title = {{Evaluation of Risk Assessments and Management for Landscape Engineering}},
Booktitle = {{2015 SEVENTH INTERNATIONAL CONFERENCE ON MEASURING TECHNOLOGY AND
   MECHATRONICS AUTOMATION (ICMTMA 2015)}},
Series = {{International Conference on Measuring Technology and Mechatronics
   Automation}},
Year = {{2015}},
Pages = {{470-472}},
Note = {{Seventh International Conference on Measuring Technology and
   Mechatronics Automation (ICMTMA 2015), Nanchang, PEOPLES R CHINA, JUN
   13-14, 2015}},
Organization = {{CPS}},
Abstract = {{Security of landscape engineering includes ecological security and
   personal safety. In this paper, two aspects of qualitative risk will be
   assessed in landscape engineering. In terms of ecological security, it
   has some major risks which includes department of community stability,
   soil quality which cannot meet the requirements of plant growth and
   plant pest disasters. In the process of construction, risks include fall
   from high-altitude, mechanical injury, electric shock and improper use
   of pesticides. In response to these security, we raise landscape
   engineering safety to measure and increase the diversity of plant
   communities, which emphasis on application of native plant species
