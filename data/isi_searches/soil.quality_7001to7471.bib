@article{ ISI:A1996VN89300006,
Author = {Shakesby, RA and Boakes, DJ and Coelho, CDA and Goncalves, AJB and
   Walsh, RPD},
Title = {{Limiting the soil degradational impacts of wildfire in pine and
   eucalyptus forests in Portugal - A comparison of alternative post-fire
   management practices}},
Journal = {{APPLIED GEOGRAPHY}},
Year = {{1996}},
Volume = {{16}},
Number = {{4}},
Pages = {{337-355}},
Month = {{OCT}},
Abstract = {{In newly burnt and unburnt pine and eucalyptus forest in Portugal,
   overland flow and soil losses were monitored to assess the impacts of
   the following post-fire treatments: application of different quantities
   of logging litter; rip-ploughing compared with minimum tillage prior to
   planting eucalyptus seedlings; and clearance of pine needles and
   vegetation. Eucalyptus logging litter reduced soil losses by up to 95
   per cent. The impact of pine logging litter was equivocal, but removal
   of pine needles increased soil losses elevenfold. Implications for soil
   longevity, soil quality and land management strategy are discussed.
   Copyright (C) 1996 Elsevier Science Ltd}},
