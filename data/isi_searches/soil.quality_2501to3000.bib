@article{ ISI:000326768200033,
Author = {Darilmaz, Enis and Kontas, Aynur and Uluturhan, Esin and Akcali, Idil
   and Altay, Oya},
Title = {{Spatial variations in polycyclic aromatic hydrocarbons concentrations at
   surface sediments from the Cyprus (Eastern Mediterranean): Relation to
   ecological risk assessment}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2013}},
Volume = {{75}},
Number = {{1-2}},
Pages = {{174-181}},
Month = {{OCT 15}},
Abstract = {{The objective of the present study was to evaluate the distribution,
   sources, origins, and environmental risk assessment of polycyclic
   aromatic hydrocarbons (16 US EPA priority pollutants) pollution in 23
   surface sediments from Cyprus coast. The mean total polycyclic aromatic
   hydrocarbons (PAHs) concentrations in the sediments from Gem! Konagi,
   Gime and Gazi Magusa areas were found 47, 52 and 50 ng/g, respectively.
   Molecular ratios and principle component analysis indicated that PAH
   pollution originated mainly from fossil sources, with higher pyrolytic
   contributions. The 2-3 ring PAHs were dominant in Cyprus sediments.
   Concentrations of PAHs observed in this study were compared with
   available soil quality guidelines and the concentrations were lower than
   the guideline values. The guideline values suggested that the Cyprus
   sediments were likely to be not contaminated by toxic PAH compounds. (C)
   2013 Elsevier Ltd. All rights reserved.}},
