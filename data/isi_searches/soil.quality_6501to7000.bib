@article{ ISI:000085200100008,
Author = {Klumpp, G and Furlan, CM and Domingos, M and Klumpp, A},
Title = {{Response of stress indicators and growth parameters of Tibouchina
   pulchra Cogn. exposed to air and soil pollution near the industrial
   complex of Cubatao, Brazil}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
