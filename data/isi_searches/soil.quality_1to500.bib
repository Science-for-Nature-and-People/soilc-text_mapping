@article{ ISI:000396210500004,
Author = {Li Qingxue and Jia Zhiqing and Liu Tao and Feng Lili and He Lingxianzi},
Title = {{Effects of different plantation types on soil properties after
   vegetation restoration in an alpine sandy land on the Tibetan Plateau,
   China}},
Journal = {{JOURNAL OF ARID LAND}},
