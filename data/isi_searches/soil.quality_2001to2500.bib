@article{ ISI:000347270600008,
Author = {Janku, Jaroslava and Kucerova, Dagmar and Houska, Jakub and Kozak, Josef
   and Rubesova, Adela},
Title = {{The Evaluation of Degraded Land by Application of the Contingent Method}},
