@article{ ISI:000342210700011,
Author = {Pandey, Bhanu and Agrawal, Madhoolika and Singh, Siddharth},
Title = {{Coal mining activities change plant community structure due to air
   pollution and soil degradation}},
Journal = {{ECOTOXICOLOGY}},
Year = {{2014}},
Volume = {{23}},
