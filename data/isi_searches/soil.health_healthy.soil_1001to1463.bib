@article{ ISI:000400860200012,
Author = {Winkler, Alexander J. and Alfonso Dominguez-Nunez, Jose and Aranaz,
   Inmaculada and Poza-Carrion, Cesar and Ramonell, Katrina and Somerville,
   Shauna and Berrocal-Lobo, Marta},
Title = {{Short-Chain Chitin Oligomers: Promoters of Plant Growth}},
