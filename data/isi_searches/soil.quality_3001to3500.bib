@inproceedings{ ISI:000313661400034,
Author = {Daughtry, C. S. T. and Beeson, P. C. and Milak, S. and Akhmedov, B. and
   Sadeghi, A. M. and Hunt, Jr., E. R. and Tomer, M. D.},
Editor = {{Neale, CMU and Maltese, A}},
Title = {{Assessing the Extent of Conservation Tillage In Agricultural Landscapes}},
Booktitle = {{REMOTE SENSING FOR AGRICULTURE, ECOSYSTEMS, AND HYDROLOGY XIV}},
