@article{ ISI:000364498300004,
Author = {Li, Jianqiang and Zhang, Hongjiang and Chen, Qibo and Zhou, Hongfeng},
Title = {{An analysis of soil fractal dimension in a sloping hedgerow agroforestry
   system in the Three Gorges Reservoir Area, China}},
Journal = {{AGROFORESTRY SYSTEMS}},
