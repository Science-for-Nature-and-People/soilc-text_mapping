@article{ ISI:000245152500002,
Author = {Fidalski, Jonez and Tormena, Cassio Antonio and Scapim, Carlos Alberto},
Title = {{Vertical and horizontal spatialization of quality indicators of a typic
   haplortox under citrus}},
