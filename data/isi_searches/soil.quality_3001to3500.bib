@article{ ISI:000310307900004,
Author = {Bijay-Singh and Varinderpal-Singh},
Title = {{Productivity and fertility of soils in the Indo-Gangetic Plains of South
   Asia}},
Journal = {{ARCHIVES OF AGRONOMY AND SOIL SCIENCE}},
Year = {{2012}},
Volume = {{58}},
