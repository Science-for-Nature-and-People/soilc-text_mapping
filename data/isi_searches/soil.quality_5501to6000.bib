@inproceedings{ ISI:000235640900011,
Author = {Gristina, L and Ferrotti, F and Poma, I and Saladino, S and Barbagallo,
   MG and Costanza, P},
Editor = {{Cano, AF and Silla, RO and Mermut, AR}},
Title = {{Management of subterranean clover, annual medic and vetch for Sicilian
   vineyard sustainability}},
Booktitle = {{SUSTAINABLE USE AND MANAGEMENT OF SOILS - ARID AND SEMIARID REGIONS}},
Series = {{ADVANCES IN GEOECOLOGY}},
Year = {{2005}},
Volume = {{36}},
Pages = {{103-112}},
Note = {{International Symposium on Sustainable Use and Management of Soils in
   Arid and Semiarid Regions, Tech Univ, Muralla del Mar Campus, Cartagena,
   SPAIN, SEP 22-26, 2002}},
Abstract = {{During 1999-2001 two vineyards, located in SW Sicily (37 degrees 60
   degrees N, 12 degrees 30 degrees E), were subjected to five different
   soil managements: ``conventional{''} soil management using up to eight
   soil-plowing operations with a five-furrow plow; natural cover cropping,
   vetch cover cropping (Vicia sativa L.); subdover cover cropping
   (Trifolium subterraneum var. Clare); and medic cover cropping (Medicago
   polymorpha var. Santiago). In the layer 0-25 cm, soil moisture, total N,
   NO(3), NH(4), PMN (potential mineralization of nitrogen) and soil
   organic matter were determined. Fresh and dry biomass weight, and total
   nitrogen percentage were determined on nine 1 m(2) subplots for each
   treatment. Soil erosion was measured on a rainfed vineyard with a 35\%
   slope, using Gerlach equipment. For each treatment the main productive,
   vegetative, and qualitative parameters were recorded on nine uniform and
   representative vines, to evaluate the effects of soil management
   techniques. Management techniques have strongly influenced the soil
   quality. Vineyards with leguminous crops improved the soil fertility,
   increased soil organic matter, and NO(3)-N. Covered crops proved to be
   very effective to control soil erosion in the area.}},
