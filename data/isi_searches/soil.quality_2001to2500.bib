@article{ ISI:000332698500015,
Author = {Reeves, J. and Cheng, Z. and Kovach, J. and Kleinhenz, M. D. and Grewal,
   P. S.},
Title = {{Quantifying soil health and tomato crop productivity in urban community
   and market gardens}},
Journal = {{URBAN ECOSYSTEMS}},
