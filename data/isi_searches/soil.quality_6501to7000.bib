@article{ ISI:000089356800060,
Author = {Southorn, N and Cattle, S},
Title = {{Monitoring soil quality for Central Tablelands grazing systems}},
Journal = {{COMMUNICATIONS IN SOIL SCIENCE AND PLANT ANALYSIS}},
Year = {{2000}},
Volume = {{31}},
