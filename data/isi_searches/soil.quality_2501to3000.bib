@article{ ISI:000314640500003,
Author = {Liu, Chang and Cui, Jun and Jiang, Guofu and Chen, Xiaofeng and Wang, Li
   and Fang, Changming},
Title = {{Soil Heavy Metal Pollution Assessment Near the Largest Landfill of China}},
Journal = {{SOIL \& SEDIMENT CONTAMINATION}},
