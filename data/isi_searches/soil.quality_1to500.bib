@article{ ISI:000396347500007,
Author = {Kim, Sung Chul and Hong, Young Kyu and Oh, Se Jin and Oh, Seung Min and
   Lee, Sang Phil and Kim, Do Hyung and Yang, Jae E.},
Title = {{Effect of chemical amendments on remediation of potentially toxic trace
   elements (PTEs) and soil quality improvement in paddy fields}},
