@article{ ISI:A1992JU04300004,
Author = {HONEK, A},
Title = {{THE EFFECT OF DEFOLIATION ON THE ABUNDANCE OF THE CEREAL APHID,
   SITOBION-AVENAE (HOMOPTERA, APHIDIDAE), ON THE EARS OF WINTER-WHEAT}},
Journal = {{ACTA ENTOMOLOGICA BOHEMOSLOVACA}},
Year = {{1992}},
Volume = {{89}},
Number = {{4}},
Pages = {{275-280}},
Abstract = {{Numbers of Sitobion avenae (F.) on the ears of control and defoliated
   wheat plants were investigated in 11 experimental plots in crops of
   three winter wheat cultivars. Plant stature between plots varied due to
   soil quality or quantity of nitrogen fertilizer. The plants were
   defoliated after earing and aphid numbers on the ears of defoliated and
   adjacent control plants were checked at 5-6 clay intervals. Maximum
   numbers of aphids per ear were up to 2.5 times higher, and aphid loads
   up to 10 times greater on defoliated than control plants. The
   differences were proportional to the leaf area removed, and to the
   compensatory growth shown by the host plant. The increase in aphid
   numbers was directly proportional to the increase in chaff mass per
   seed. The physiological and biochemical changes in the defoliated plants
   that may cause the variation in aphid performance are discussed.}},
