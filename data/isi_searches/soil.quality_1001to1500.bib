@article{ ISI:000359160800008,
Author = {Ciccolini, Valentina and Bonari, Enrico and Pellegrino, Elisa},
Title = {{Land-use intensity and soil properties shape the composition of fungal
   communities in Mediterranean peaty soils drained for agricultural
   purposes}},
Journal = {{BIOLOGY AND FERTILITY OF SOILS}},
