@article{ ISI:000405400700012,
Author = {Mahlangeni, Nomfundo Thobeka and Moodley, Roshila and Jonnalagadda,
   Sreekantha Babu},
Title = {{Elemental Analysis of Edible Mountain Nettle (Obetia tenax) and the
   Influence of Soil on Its Chemical Composition}},
