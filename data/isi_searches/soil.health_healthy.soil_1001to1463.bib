@article{ ISI:000385511400013,
Author = {Vazquez, Maria B. and Amodeo, Martin R. and Bianchinotti, Maria V.},
Title = {{Fungal biomass estimation in soils from southwestern Buenos Aires
   province(Argentina) using calcofluor white stain}},
Journal = {{REVISTA ARGENTINA DE MICROBIOLOGIA}},
