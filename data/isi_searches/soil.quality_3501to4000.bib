@article{ ISI:000295863600008,
Author = {Pereira, B. F. F. and He, Z. L. and Stoffella, P. J. and Melfi, A. J.},
Title = {{Reclaimed wastewater: Effects on citrus nutrition}},
Journal = {{AGRICULTURAL WATER MANAGEMENT}},
