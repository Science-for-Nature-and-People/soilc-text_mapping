@article{ ISI:000355126000006,
Author = {Burtea, Mariana Carmen and Sandu, Ioan Gabriel and Cioromele, Gabriela
   Alina and Bordei, Marian and Ciurea, Aurel and Romanescu, Gheorghe},
Title = {{Sustainable Exploitation of Ecosystems on the Big Island of Braila}},
