@inproceedings{ ISI:000175523300020,
Author = {Pearson, AJ and Bloomer, DJ and Grant, M},
Editor = {{Hampton, JG and Pollock, KM}},
Title = {{Impact of tillage system on sweet corn yield and some soil properties}},
Booktitle = {{AGRONOMY NEW ZEALAND, PROCEEDINGS}},
Series = {{PROCEEDINGS OF THE AGRONOMY SOCIETY OF NEW ZEALAND}},
Year = {{2000}},
Volume = {{30}},
Pages = {{139-142}},
Note = {{30th Joint NZIAS/NZSHS Annual Conference, PALMERSTON, NEW ZEALAND, 2000}},
Organization = {{NZIAS; NZSHS}},
Abstract = {{A single season and non-replicated comparison of conventional and
   no-tillage was evaluated by measuring sweet corn (Zea mays L.) crop
   yield and soil properties on a wind erosion prone soil in Hawke's Bay.
   Tillage treatment did not affect plant population or crop yield. By
   harvest, indicators of soil quality such as earthworm numbers, microbial
   biomass carbon, soil aggregate size and soil aggregrate stability were
   higher under no-tillage than conventional tillage.}},
