Title = {{Maize productivity and profitability in Conservation Agriculture systems
   across agro-ecological regions in Zimbabwe: A review of knowledge and
   practice}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{2016}},
Volume = {{220}},
