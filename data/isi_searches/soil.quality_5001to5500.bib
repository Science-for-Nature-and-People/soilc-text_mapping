@article{ ISI:000253920300004,
Author = {Trung, Nguyen Dai and Verdoodt, Ann and Dusar, Michiel and Van, Tran Tan
   and Van Ranst, Eric},
Title = {{Evaluating ethnopedological knowledge systems for chassifying soil
   quality. A case study in Bo hamlet with Muong people of northern Vietnam}},
