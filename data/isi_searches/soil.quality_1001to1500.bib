@article{ ISI:000352588500059,
Author = {Luo, Bang-lin and Chen, Xiao-yan and Ding, Lin-qiao and Huang, Yu-han
   and Zhou, Ji and Yang, Tian-tian},
Title = {{Response Characteristics of Soil Fractal Features to Different Land Uses
   in Typical Purple Soil Watershed}},
