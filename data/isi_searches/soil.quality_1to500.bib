@article{ ISI:000405255300013,
Author = {Guimaraes, Rachel M. L. and Lamande, Mathieu and Munkholm, Lars J. and
   Ball, Bruce C. and Keller, Thomas},
Title = {{Opportunities and future directions for visual soil evaluation methods
   in soil structure research}},
