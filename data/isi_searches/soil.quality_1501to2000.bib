@article{ ISI:000344595900008,
Author = {Luo, Ling and Gu, Ji-Dong},
Title = {{Seasonal Variability of Extracellular Enzymes Involved in Carbon
   Mineralization in Sediment of a Subtropical Mangrove Wetland}},
Journal = {{GEOMICROBIOLOGY JOURNAL}},
Year = {{2015}},
Volume = {{32}},
Number = {{1}},
Pages = {{68-76}},
Month = {{JAN 2}},
Abstract = {{This study investigated seasonal variations of extracellular enzymatic
   activity in coastal mangrove sediment of the Mai Po Nature Reserve in
   Hong Kong, China. The results showed that enzyme activities varied
   widely with the two distinctive seasons, and negative relationship was
   found between oxidative and hydrolytic enzymes (p < 0.05). Moreover, pH,
   temperature, soluble phenolics, total organic carbon, phosphorus, and
   nitrogen significantly influenced enzyme activities. The seasonal
   variability of enzymes can be explained by the variations of
   environmental factors caused by the anthropogenic and terrestrial
   inputs, and provide information for a better understanding on the
   dynamics of sediment organic carbon in this ecosystem.}},
