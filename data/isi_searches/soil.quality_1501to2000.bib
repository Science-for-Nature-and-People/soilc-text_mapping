@article{ ISI:000346648400010,
Author = {Hueso-Gonzalez, Paloma and Francisco Martinez-Murillo, Juan and Damian
   Ruiz-Sinoga, Jose},
Title = {{THE IMPACT OF ORGANIC AMENDMENTS ON FOREST SOIL PROPERTIES UNDER
   MEDITERRANEAN CLIMATIC CONDITIONS}},
