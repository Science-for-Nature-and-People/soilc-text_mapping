@article{ ISI:000245845600016,
Author = {Nourbakhsh, Farshid},
Title = {{Decoupling of soil biological properties by deforestation}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{2007}},
Volume = {{121}},
Number = {{4}},
Pages = {{435-438}},
Month = {{AUG}},
Abstract = {{Soil management practices can influence both the level and quality of
   soil organic matter. The effects of oak (Quercus brantii Lindl)
   deforestation on soil organic C (SOC), soil basal respiration (SBR),
   potentially mineralizable N (PMN), Soil L-asparaginase activity (LAS)
   and their association were investigated. Soil samples were collected
   from protected forest and deforested counterparts of four adjacent sites
   in Lordegan region, west central Iran. Deforestation decreased SOC, SBR,
   PMN and LAS by 63, 62, 57 and 61\%, respectively, and reduced the
   correlation coefficients among the bio-indicators to insignificant
   levels. It can be concluded that deforestation not only degraded the
   soil biological properties but also decreased the degree of correlation
   among them. (c) 2006 Elsevier B.V. All rights reserved.}},
