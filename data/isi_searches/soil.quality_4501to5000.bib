@inproceedings{ ISI:000271258000337,
Author = {He Zhenli and Fan Jinghua and Yang Xiaoe and Stoffella, Peter J.},
Editor = {{Li, SC and Wang, YJ and Cao, FX and Huang, P and Zhang, Y}},
Title = {{Effect of Copper Contamination on Soil Quality and Transport to the
   Environment}},
