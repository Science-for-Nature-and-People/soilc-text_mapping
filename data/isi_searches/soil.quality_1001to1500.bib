@article{ ISI:000352659900009,
Author = {Duan, Yonghong and Shen, Guofeng and Tao, Shu and Hong, Jianping and
   Chen, Yuanchen and Xue, Miao and Li, Tongchao and Su, Shu and Shen,
   Huizhong and Fu, Xiaofang and Meng, Qingchun and Zhang, Jing and Zhang,
   Bing and Han, Xiaoying and Song, Kang},
Title = {{Characteristics of polycyclic aromatic hydrocarbons in agricultural
   soils at a typical coke production base in Shanxi, China}},
Journal = {{CHEMOSPHERE}},
Year = {{2015}},
Volume = {{127}},
Pages = {{64-69}},
Month = {{MAY}},
Abstract = {{There is wide concern about polycyclic aromatic hydrocarbons (PAHs)
   because of their carcinogenic and mutagenic potential. The coking
   industry is an important source of PAHs. In this study, 36 arable soil
   samples, a sensitive medium from the perspective of food safety and
   health, were collected from one of the largest coke production bases in
   China. The concentration of total 21 PAHs ranged from 294 to 1665 ng
   g(-1), with a mean of 822 +/- 355 ng g(-1). Approximately 60\% of the
   soil samples were heavily polluted with the level higher than 600 ng
   g(-1). Particularly high abundances of high molecular weight PAHs were
   found, and the calculated BaPeq was as high as 54.3 ng g-(-1). Soil PAH
   levels were positively correlated with soil organic matter content. The
   soil PAHs were from complex mixture sources, and high-temperature
   pyrogenic sources were most likely responsible for the heavy PAR
   contamination. Effective control strategies and probable remediation
   approaches should be proposed to improve soil quality. (C) 2015 Elsevier
