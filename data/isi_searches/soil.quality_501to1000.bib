@article{ ISI:000371714000002,
Author = {Adebiyi, Jelili and Olabisi, Laura Schmitt and Snapp, Sieglinde},
Title = {{Understanding perennial wheat adoption as a transformative technology:
   evidence from the literature and farmers}},
Journal = {{RENEWABLE AGRICULTURE AND FOOD SYSTEMS}},
Year = {{2016}},
