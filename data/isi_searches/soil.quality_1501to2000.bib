@article{ ISI:000348974000005,
Author = {Bestelmeyer, Brandon T. and Okin, Gregory S. and Duniway, Michael C. and
   Archer, Steven R. and Sayre, Nathan F. and Williamson, Jebediah C. and
   Herrick, Jeffrey E.},
Title = {{Desertification, land use, and the transformation of global drylands}},
Journal = {{FRONTIERS IN ECOLOGY AND THE ENVIRONMENT}},
Year = {{2015}},
Volume = {{13}},
Number = {{1, SI}},
Pages = {{28-36}},
Month = {{FEB}},
Abstract = {{Desertification is an escalating concern in global drylands, yet
   assessments to guide management and policy responses are limited by
   ambiguity concerning the definition of ``desertification{''} and what
   processes are involved. To improve clarity, we propose that assessments
   of desertification and land transformation be placed within a state
   change-land-use change (SC-LUC) framework. This framework considers
   desertification as state changes occurring within the context of
   particular land uses (eg rangeland, cropland) That interact with
   land-use change. State changes that can be readily reversed are
   distinguished from regime shifts, which are state changes involving
   persistent alterations to vegetation or soil properties. Pressures
   driving the transformation of rangelands to other types of land uses may
   be low, fluctuating, or high, and may influence and be influenced by
   state change. We discuss how the SC-LUC perspective can guide more
   effective assessment of desertification and management of drylands.}},
