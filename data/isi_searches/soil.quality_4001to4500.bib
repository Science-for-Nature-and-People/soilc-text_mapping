@article{ ISI:000286164100008,
Author = {Czaban, Janusz and Wroblewska, Barbara and Niedzwiecki, Jacek and Sulek,
   Alicja},
Title = {{Relationships between Numbers of Microbial Communities in Polish
   Agricultural Soils and Properties of these Soils, paying Special
   Attention to Xerophilic/Xerotolerant Fungi}},
Journal = {{POLISH JOURNAL OF ENVIRONMENTAL STUDIES}},
Year = {{2010}},
Volume = {{19}},
Number = {{6}},
Pages = {{1171-1183}},
Abstract = {{Our study was conducted with seven typical Polish soils very different
   in their physical and chemical features and fertility (a Phaeozem, an
   Eutric Fluvisol, three Etude Cambisols, and two Dystric Cambisols).
   Samples of the soils were taken from a plot experiment established in
   Pulawy at the end of the 19(th) century and cultivated only by hand
   tillage. Correlation analyses of the results obtained suggest that soil
   microorganisms differing in their water requirements were associated
   with different soil environments: bacteria with small particles and
   small pores usually filled with water, nonxerophilic fungi with medium
   particles and medium pores which are usually the region of near the
   air-water interface, and xerotolerant/xerophilic fungi (especially
   penicillia) predominantly with big particles and big pores usually
   filled with air. Moreover, CFU of xerotolerant/xerophilic fungi and
   particularly formulated indices of xerotolerance of the soil fungal
   communities (in contrast to CFU of bacteria), were negatively correlated
   with water content, SOM content, soil pH, and an index of soil quality.}},
