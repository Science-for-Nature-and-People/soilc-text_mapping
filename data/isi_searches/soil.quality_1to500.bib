@article{ ISI:000390734100114,
Author = {Yang, Yang and Chen, Weiping and Wang, Meie and Peng, Chi},
Title = {{Regional accumulation characteristics of cadmium in vegetables:
   Influencing factors, transfer model and indication of soil threshold
   content}},
Journal = {{ENVIRONMENTAL POLLUTION}},
Year = {{2016}},
