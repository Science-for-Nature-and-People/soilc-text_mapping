@article{ ISI:000089420800006,
Author = {De Bakker, D and Maelfait, JP and Hendrickx, F and Van Waesberghe, D and
   De Vos, B and Thys, S and De Bruyn, L},
Title = {{A first analysis on the relationship between forest soil quality and
   spider (Araneae) communities of Flemish forest stands}},
Journal = {{EKOLOGIA-BRATISLAVA}},
Year = {{2000}},
