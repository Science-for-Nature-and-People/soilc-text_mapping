@article{ ISI:000189004900015,
Author = {Palumbo, AV and McCarthy, JF and Amonette, JE and Fisher, LS and
   Wullschleger, SD and Daniels, WL},
Title = {{Prospects for enhancing carbon sequestration and reclamation of degraded
   lands with fossil-fuel combustion by-products}},
