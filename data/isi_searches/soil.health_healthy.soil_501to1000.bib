@article{ ISI:000293668400016,
Author = {Reddy, R. Uma and Reddy, M. Suryanarayan},
Title = {{Availability of major nutrients in soil as influenced by integrated
   nutrient management in tomato-onion cropping system}},
Journal = {{INDIAN JOURNAL OF HORTICULTURE}},
Year = {{2011}},
Volume = {{68}},
Number = {{2}},
Pages = {{224-228}},
Month = {{JUN}},
Abstract = {{Integrated use of organic manures and inorganic fertilizers was studied
   for crop response in an experiment based on tomato-onion cropping
   system. Application of organic manures with inorganic fertilizers
   significantly improved the availability of N, P, and K in soil. The
   availability of major nutrients in soil increased with increase in level
   of organic manures application and with no fertilizer registered maximum
   but on par with 50 and 75 per cent manure levels while the 25 per cent
   level of organic manure recorded minimum. Based on the results of this
   study it could be concluded that the combined use of organic manures and
   inorganic fertilizers is suitable for sustaining yield and maintaining
   soil health.}},
