@article{ ISI:000244331700008,
Author = {Nourbakhsh, F. and Monreal, C. M.},
Title = {{Urease activity as affected by cultivation and soil depth: A kinetic
   approach}},
Journal = {{AGROCHIMICA}},
Year = {{2006}},
Volume = {{50}},
Number = {{1-2}},
Pages = {{72-76}},
Month = {{JAN-APR}},
Abstract = {{Soil enzyme activities have received considerable attention in recent
   years because they can be used as indicators of soil quality. A
   comparative study was conducted to investigate the effects of 20 years
   of irrigated alfalfa cultivation on kinetic parameters of urease (EC
   3.5.1.5). Replicated soil cores were collected from uncultivated (UCU)
   and cultivated (CU) counterparts to a depth of 50 cm in 10 cm
   increments. Results showed that soil organic C was higher in all depths
   of CU comparing UCU soil. Vmax values of soil urease in both
   counterparts decreased as depth increased. A significant correlation was
   observed between Vmax and soil organic C. The Km values of CU soils were
   higher than those of UCU soils and decreased with depth. A significant
   correlation was observed between Km and soil organic C. The irrigated
   cultivation of alfalfa in the region increased the carbon storage of
   soil, enhanced the total amount of soil urease and influenced the enzyme
   status.}},
