@article{ ISI:000303385800012,
Author = {Vaneeckhaute, C. and Meers, E. and Michels, E. and Christiaens, P. and
   Tack, F. M. G.},
Title = {{Fate of Macronutrients in Water Treatment of Digestate Using Vibrating
   Reversed Osmosis}},
