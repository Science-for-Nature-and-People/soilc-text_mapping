@article{ ISI:000275543300002,
Author = {Dayton, Elizabeth A. and Whitacre, Shane D. and Dungan, Robert S. and
   Basta, Nicholas T.},
Title = {{Characterization of physical and chemical properties of spent foundry
   sands pertinent to beneficial use in manufactured soils}},
Journal = {{PLANT AND SOIL}},
