@article{ ISI:000242542800002,
Author = {Chen, Jian and Yu, Zhenrong and Ouyang, Jinliang and van Mensvoort, M.
   E. F.},
Title = {{Factors affecting soil quality changes in the North China Plain: A case
   study of Quzhou County}},
Journal = {{AGRICULTURAL SYSTEMS}},
