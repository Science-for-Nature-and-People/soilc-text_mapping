@article{ ISI:000315388200002,
Author = {McCormack, Sarah A. and Ostle, Nick and Bardgett, Richard D. and
   Hopkins, David W. and Vanbergen, Adam J.},
Title = {{Biochar in bioenergy cropping systems: impacts on soil faunal
   communities and linked ecosystem processes}},
Journal = {{GLOBAL CHANGE BIOLOGY BIOENERGY}},
