@article{ ISI:000286787900014,
Author = {Wu, Shan and Xia, Xinghui and Yang, Lingyan and Liu, Hong},
Title = {{Distribution, source and risk assessment of polychlorinated biphenyls
   (PCBs) in urban soils of Beijing, China}},
Journal = {{CHEMOSPHERE}},
Year = {{2011}},
Volume = {{82}},
Number = {{5}},
Pages = {{732-738}},
Month = {{JAN}},
Abstract = {{The concentration of PCBs in urban soils of Beijing across six land
   types including business districts (BD), cultural and educational areas
   (CE), classical gardens (CG), public green (PG), residential areas (RA),
   and roadsides (RD) was investigated. The total concentrations of PCBs
   ranged from below method detection limit (M.D.L.) to 37.11 x 10(-3) mg
   kg(-1) dry weight (mean: 11.70 x 10(-3) mg kg(-1); median: 13.28 x
   10(-3) mg kg(-1)), which was at a medium level compared with other
   cities around the world. In general, the levels of PCBs decreased from
   the center of Beijing city to the suburb, suggesting the increasing PCBs
   concentrations with the age of the urban area. For different types of
   land use, the highest level of PCBs was observed in CG soils which have
   the oldest age, and the homologue profiles were similar, which were
   predominated by lowly chlorinated congeners including di-, tri- and
   tetra-CBs. Principal component analysis indicated that local sources
   such as Aroclor 1016, 1242, and 1248 were important sources in addition
   to long-range transport of PCBs all over the world. Although the toxic
   equivalency (TEQ) concentrations of dioxin-like PCBs in all soil samples
   of Beijing were higher than those in other urban areas of China, they
   met the Canadian soil quality standard. (C) 2010 Elsevier Ltd. All
   rights reserved.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
