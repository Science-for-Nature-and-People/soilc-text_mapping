
@article{ ISI:000382269000020,
Author = {Usman, M. and Hanna, K. and Haderlein, S.},
Title = {{Fenton oxidation to remediate PAHs in contaminated soils: A critical
   review of major limitations and counter-strategies}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
