@inproceedings{ ISI:000235640900016,
Author = {Bienes, R and More, A and Marques, MJ and Moreiro, S and Nicolau, JM},
Editor = {{Cano, AF and Silla, RO and Mermut, AR}},
Title = {{Efficiency of different plant cover to control water erosion in central
   Spain}},
Booktitle = {{SUSTAINABLE USE AND MANAGEMENT OF SOILS - ARID AND SEMIARID REGIONS}},
Series = {{ADVANCES IN GEOECOLOGY}},
Year = {{2005}},
Volume = {{36}},
Pages = {{155-162}},
Note = {{International Symposium on Sustainable Use and Management of Soils in
   Arid and Semiarid Regions, Tech Univ, Muralla del Mar Campus, Cartagena,
   SPAIN, SEP 22-26, 2002}},
Abstract = {{In Spain, soil quality has been found to change in regions with gradual
   abandonment of agricultural lands. The response of these lands following
   abandonment is variable. In some cases the runoff coefficient decreases,
   but sometimes may increase due to the gradual loss of plant cover under
   semi-arid conditions with about 500 mm rainfall. The latter situation is
   now considered a serious environmental concern in Spain. The aim of this
   work was to study water erosion and runoff in the center of the Iberian
   Peninsula. The study was undertaken for three consecutive years on
   experimental USLE-type plots where soil was maintained without
   vegetation, or covered by natural vegetation or sowed with barley.
   The water from the runoff was collected in each plot following each
   rainfall. The influence of the pluviometer, the maximum intensity in 30
   minutes, and the kinetic energy of the rain were also measured. The
   biggest runoff coefficient corresponded to bare soil and there was a
   gradual increase with time. At the end of the experiment, in some
   locations, the value of the runoff coefficient reached six times that of
   the initial value.}},
