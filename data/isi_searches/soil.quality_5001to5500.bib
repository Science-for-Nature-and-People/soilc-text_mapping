@article{ ISI:000254747100012,
Author = {Zaharia, C. and Surpateanu, M. and Macoveanu, M.},
Title = {{Assessment of environmental impact generated by municipal waste
   deposition into a Romanian landfill}},
Journal = {{JOURNAL OF ENVIRONMENTAL PROTECTION AND ECOLOGY}},
Year = {{2007}},
Volume = {{8}},
Number = {{2}},
Pages = {{332-345}},
Abstract = {{The deposition of municipal solid wastes is a technological process
   considered to have negative influence on the environment quality being a
   potential pollution source. The evaluation of air, water and soil
   quality during the solid waste deposition activities has a great
   importance to express the environment pollution level into the area of
   deposition and proximity. In the northen region of Romania (northen
   Moldova), the municipal waste deposition was done directly on soil and
   subsoil, into special area organised at the extremity of the town, named
   municipal landfill. The environmental impact of technological process
   for municipal waste deposition (usually named environmental impact
   assessment, EIA) was preliminary evaluated using the global pollution
   index (I,,). For the studied waste landfill (ca 10.5 ha) that offers
   deposition facilities for households and assimilable wastes derived from
   shops, offices and other commercial units (from ca. 130 000 inhabitants
   and 171 economic agents and institutes), it seems that the environment
   was modified by waste deposition technological process generating
   distress to life forms (I-PG value close to 3.44).}},
