@incollection{ ISI:000370798900008,
Author = {Wang, Xiuying and Kemanian, Armen R. and Williams, Jimmy R.},
Editor = {{Ahuja, LR and Ma, L}},
Title = {{Special Features of the EPIC and APEX Modeling Package and Procedures
   for Parameterization, Calibration, Validation, and Applications}},
Booktitle = {{METHODS OF INTRODUCING SYSTEM MODELS INTO AGRICULTURAL RESEARCH}},
