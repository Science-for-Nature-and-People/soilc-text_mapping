@inproceedings{ ISI:000337169400014,
Author = {Askari, Mohammad Sadegh and Holden, Nicholas M.},
Editor = {{Hartemink, AE and McSweeney, K}},
Title = {{Rapid Evaluation of Soil Quality Based on Soil Carbon Reflectance}},
Booktitle = {{SOIL CARBON}},
Series = {{Progress in Soil Science}},
