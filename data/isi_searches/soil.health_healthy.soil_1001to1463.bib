@article{ ISI:000363820100023,
Author = {Bhattacharyya, T. and Chandran, P. and Ray, S. K. and Pal, D. K. and
   Mandal, C. and Mandal, D. K.},
Title = {{Distribution of zeolitic soils in India}},
Journal = {{CURRENT SCIENCE}},
