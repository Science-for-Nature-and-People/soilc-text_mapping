@inproceedings{ ISI:000359291100014,
Author = {Sheffield, K. and Morse-McNabb, E.},
Book-Group-Author = {{IOP}},
Title = {{Using satellite imagery to asses trends in soil and crop productivity
   across landscapes}},
Booktitle = {{SOIL CHANGE MATTERS 2014}},
