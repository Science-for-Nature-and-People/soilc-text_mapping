@article{ ISI:000187634000004,
Author = {Svendsen, C and Spurgeon, DJ and Hankard, PK and Weeks, JM},
Title = {{A review of lysosomal membrane stability measured by neutral red
   retention: is it a workable earthworm biomarker?}},
Journal = {{ECOTOXICOLOGY AND ENVIRONMENTAL SAFETY}},
Year = {{2004}},
Volume = {{57}},
Number = {{1}},
Pages = {{20-29}},
Month = {{JAN}},
Note = {{3rd International Workshop on Earthworm Ecotoxicology, UNIV AARHUS,
   AARHUS, DENMARK, AUG 26-29, 2001}},
Organization = {{Inst Terrestrial Ecol; Natl Environm Res Inst; Univ Aarhus, Inst Biol}},
Abstract = {{The ability of biomarkers to integrate effects of chemicals on biota has
   lead to increased calls for their application in assessing the status of
   polluted ecosystems. In tandem there has been an increase in our
   knowledge of the ecophysiological responses of keystone species to
   pollutants, which has allowed the development of a number of promising
   methods. In contrast to the number of biomarker development studies, the
   number of biomarker validation studies has remained limited. This paper
   redresses this imbalance by drawing together data from studies that have
   used the earthworm lysosomal membrane stability response (measured using
   the neutral red retention assay). This review first gives a short
   history of the biomarker's development. Second, it sets published
   applications of the technique against established criteria for a
   ``good{''} biomarker (i.e., dose-response relationship, sensitivity,
   ecological relevance, confounding factors, chemical specificity, species
   differences, time-response relationship, methodological concerns, and
   overall public/regulator confidence, and acceptance). Discussion of the
   biomarker's suitability to each criterion is followed by an overall
   evaluation of its workability for routine soil quality assessment and
   caveats for its use. (C) 2003 Elsevier Inc. All rights reserved.}},
