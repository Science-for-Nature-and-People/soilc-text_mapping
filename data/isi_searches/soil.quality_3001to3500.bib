Author = {van der Hilst, F. and Lesschen, J. P. and van Dam, J. M. C. and Riksen,
   M. and Verweij, P. A. and Sanders, J. P. M. and Faaij, A. P. C.},
Title = {{Spatial variation of environmental impacts of regional biomass chains}},
