@article{ ISI:A1997YL42700014,
Author = {Green, GP and Sunding, DL},
Title = {{Land allocation, soil quality, and the demand for irrigation technology}},
Journal = {{JOURNAL OF AGRICULTURAL AND RESOURCE ECONOMICS}},
Year = {{1997}},
Volume = {{22}},
Number = {{2}},
Pages = {{367-375}},
Month = {{DEC}},
Abstract = {{Economists have long argued that increasing the price of agricultural
   water will encourage the adoption of efficient irrigation technologies.
   This article considers the choice of irrigation systems conditional on
   prior land allocation decisions. Adoption functions for gravity and
   low-pressure irrigation technologies are estimated for citrus and
   vineyard crops using a held-level data set from California's Central
   Valley. Results show that the influence of land quality and water price
   on low-pressure technology adoption is greater for citrus than for
   vineyard crops. Consequently, the response of growers to changes in
   policy will be conditional on land allocation.}},
