@incollection{ ISI:000298225500010,
Author = {Montanari, R. and Lima, R. C. and Bonini, A. S. and Marques, L. S. and
   Minguini, R. and Carvalho, M. P. and Paz Ferreiro, J. and Costa, N. R.},
