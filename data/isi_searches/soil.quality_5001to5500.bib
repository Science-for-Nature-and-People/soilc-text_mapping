@article{ ISI:000262183700017,
Author = {Niec, Marek and Pietrzyk-Sokulska, Elzbieta and Gadek, Ryszard and
   Lisner-Skorska, Jadwiga},
Title = {{MINING HELPFUL TO ENVIRONMENT PROTECTION AND MANAGEMENT - CASE OF KIELCE
   MINERAL MINES ENTERPRISE (KKSM)}},
Journal = {{GOSPODARKA SUROWCAMI MINERALNYMI-MINERAL RESOURCES MANAGEMENT}},
Year = {{2008}},
