DOI = {{10.1016/j.catena.2013.07.009}},
ISSN = {{0341-8162}},
Keywords = {{Dissolved organic carbon; Khasi mandarin; Microbial biomass; Nutrient
   cycling; Potentially mineralizable nitrogen; Soil enzymes}},
