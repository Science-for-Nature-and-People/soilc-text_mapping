@article{ ISI:000167969400007,
Author = {Liebig, MA and Varvel, G and Doran, J},
Title = {{A simple performance-based index for assessing multiple agroecosystem
   functions}},
Journal = {{AGRONOMY JOURNAL}},
Year = {{2001}},
Volume = {{93}},
Number = {{2}},
