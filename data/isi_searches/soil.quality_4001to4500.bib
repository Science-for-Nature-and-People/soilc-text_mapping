@article{ ISI:000208665000003,
Author = {Correa, Rossini M. and Freire, Maria B. G. dos S. and Ferreira, Rinaldo
   L. C. and da Silva, Jose A. A. and Pessoa, Luiz G. M. and Miranda,
   Marcelo A. and de Melo, Diego V. M.},
Title = {{Physical attributes of soil under different uses with irrigation in
   semiarid Pernambuco}},
