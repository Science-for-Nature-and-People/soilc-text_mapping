@article{ ISI:000322128800001,
Author = {Yong-Tan and Wang, Shan-Shan and Gao, Ting-Ting and Shao, Hong-bo},
Title = {{Study on the effects of growth and development and effective composition
   accumulation of different cultivated years of Conioselinum vaginatium
   and soil quality improvement}},
Journal = {{JOURNAL OF FOOD AGRICULTURE \& ENVIRONMENT}},
Year = {{2013}},
Volume = {{11}},
Number = {{2}},
Pages = {{445-448}},
Abstract = {{The Conioselinum vaginatium is an important traditional Chinese medicine
   for wind-expelling and relieving pain. This medical material in market
   depend on artificial plants in China currently. However, study on
   relationship quality and cultivated years is little. The study was
   designed to establish an analytical method based on high-performance
   liquid chromatography (HPLC) with diode array detection (DAD) for the
   determination of ferulic acid in different cultivated years of
   Conioselinum vaginatium. The separation was performed on a C-18 column
   with mobile phase acetonitrile-0.4\% glacial acetic acid (45:55, v/v),
   UV detection at 320 nm, the flow rate of 1.0 mL min(-1), the column
   temperature of 30 degrees C. The ferulic acid sample shows a good
   linearity in the range of 0.49-8.85 mu g mL(-1). The average recovery
   was 93.8\% and the relative standard deviation (RSD) was 1.85\%. The
   method was simple, accurate and reproducible and successfully applied to
   the analysis of ferulic acid in Conioselinum vaginatium, which can be
   used for the quality control of this crude drugs. The results will be
   used for the cultivation and collection and quality control of raw plant
   material of Conioselinum vaginatium. The cultivation of this medicinal
   plants also improved soil quality.}},
