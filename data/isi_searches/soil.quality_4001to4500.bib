@inproceedings{ ISI:000378593000032,
Author = {Dreija, Kristine},
Editor = {{Treija, S and Skuja, I}},
Title = {{THE MEANING OF HISTORICAL PARKS IN THE SETTLEMENT LANDSCAPE ECOLOGY}},
Booktitle = {{RESEARCH FOR RURAL DEVELOPMENT 2011, VOL 2}},
