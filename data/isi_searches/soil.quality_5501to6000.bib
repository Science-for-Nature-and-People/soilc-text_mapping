@article{ ISI:000233768700010,
Author = {Bedano, JC and Cantu, MP and Doucet, ME},
Title = {{Abundance of soil mites (Arachnida : Acari) in a natural soil of central
   Argentina}},
Journal = {{ZOOLOGICAL STUDIES}},
