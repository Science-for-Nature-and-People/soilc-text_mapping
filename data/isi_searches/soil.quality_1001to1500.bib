@article{ ISI:000369617100010,
Author = {Frederico Pereira, Antonio Higino and Tadeu Vitorino, Antonio Carlos and
   Ferreira do Prado, Eber Augusto and Bergamin, Anderson Cristian and
   Mauad, Munir and Arantes, Heverton Ponce},
Title = {{Least Limiting Water Range and Load Bearing Capacity of Soil under Types
   of Tractor-Trailers for Mechanical Harvesting of Green Sugarcane}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
Year = {{2015}},
Volume = {{39}},
Number = {{6}},
Pages = {{1603-1610}},
Month = {{NOV-DEC}},
Abstract = {{The expansion of the sugarcane industry in Brazil has intensified the
   mechanization of agriculture and caused effects on the soil physical
   quality. The purpose of this study was to evaluate the limiting water
   range and soil bearing capacity of a Latossolo Vermelho distroferrico
   tipico (Rhodic Hapludox) under the influence of different
   tractor-trailers used in mechanical sugarcane harvesting. The experiment
   was arranged in a randomized block design with five replications. The
   treatments consisted of green sugarcane harvesting with: harvester
   without trailer (T1); harvester with two trailers with a capacity of 10
   Mg each (T2); harvester with trailer with a capacity of 20 Mg (T3) and
   harvester and truck with trailer with a capacity of 20 Mg (10 Mg per
   compartment) (T4). The least limiting water range and soil bearing
   capacity were evaluated. The transport equipment to remove the harvested
   sugarcane from the field (trailer) at harvest decreased the least
   limiting water range, reducing the structural soil quality. The truck
   trailer caused the greatest impact on the soil physical properties
   studied. The soil load bearing capacity was unaffected by the
   treatments, since the pressure of the harvester (T1) exceeded the
   pre-consolidation pressure of the soil.}},
