@inproceedings{ ISI:000359291100016,
Author = {Marchant, B. P. and Crawford, D. M. and Robinson, N. J.},
Book-Group-Author = {{IOP}},
Title = {{What can legacy datasets tell us about soil quality trends? Soil acidity
   in Victoria}},
Booktitle = {{SOIL CHANGE MATTERS 2014}},
