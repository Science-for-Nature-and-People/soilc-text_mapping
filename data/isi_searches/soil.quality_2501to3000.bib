@article{ ISI:000317335400005,
Author = {Huang, Cheng and Wang, Tianle and Luo, Yongzheng and Chen, Shi and Kim,
   Ke Chung},
Title = {{Assessing Collembola Biodiversity Under Human Influence at Three Gorges
   Area, China}},
Journal = {{ENVIRONMENTAL ENTOMOLOGY}},
