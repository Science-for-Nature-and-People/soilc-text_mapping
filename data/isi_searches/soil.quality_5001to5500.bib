@article{ ISI:000252761800008,
Author = {Hinrichs, Jan and Musshoff, Oliver and Odening, Martin},
Title = {{Economic hysteresis in hog production}},
Journal = {{APPLIED ECONOMICS}},
Year = {{2008}},
Volume = {{40}},
Number = {{3}},
Pages = {{333-340}},
Abstract = {{German hog production only responds in a very limited way to price
   fluctuations in the pork market. The hog production concentrates on a
   few regions though it is not bound to special natural conditions such as
   soil quality. Furthermore, the volume of production does not vary over
   time. Relatively high market risks, sunk costs and the flexibility of
   the decision maker to defer investments characterize decision problems
   in hog production. Thus the real option approach is chosen to explain
   the inertia in production capacity. By the use of panel data of
   specialized hog farms from the German Farm Accountancy Data Network, an
   empirical investment model is estimated. Formally, the model has the
   structure of a generalized ordered probit model. This approach allows to
   test for economic hysteresis in the adjustment of hog production
   capacity. The results confirm that uncertainty and flexibility widen the
   optimal range of inaction.}},
