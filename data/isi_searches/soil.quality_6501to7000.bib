@article{ ISI:000168401700003,
Author = {Cornu, S and Neal, C and Ambrosi, JP and Whitehead, P and Neal, M and
   Sigolo, J and Vachier, P},
Title = {{The environmental impact of heavy metals from sewage sludge in
   ferralsols (Sao Paulo, Brazil)}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2001}},
Volume = {{271}},
Number = {{1-3}},
Pages = {{27-48}},
Month = {{APR 23}},
Abstract = {{The spreading of sewage sludge on acidic ferralsol soil was simulated in
   a column experiment in order to estimate the potential impact on water
   and soil quality. Drainage and runoff water were sampled daily over a
   2-month experiment and analysed for Fe, Ni, Cu, Pb and DOG. Results show
   a clear influence of the sludge on the water quality, especially at the
   start of the experiment, increasing the export of Cu, Ni, Ph and in DOC
   from the soil, mainly in dissolved form. Nickel was usually the most
   mobile element in the sludge and in the soil. In the soil, Pb mobility
   was complex and increased drastically with time compared to the other
   elements. No significant increase in soil heavy metal content due to the
   sludge spreading was recorded on this time scale. (C) 2001 Elsevier
   Science B.V. All rights reserved.}},
