@article{ ISI:000272186900003,
Author = {Jansa, Jan and Oberholzer, Hans-Rudolf and Egli, Simon},
Title = {{Environmental determinants of the arbuscular mycorrhizal fungal
   infectivity of Swiss agricultural soils}},
