Editor = {{Biala, J and Prange, R and Raviv, M}},
Title = {{Integrating Organic Matter into Banana Plantation in North Queensland:
   the Effects on Soil Properties}},
Booktitle = {{I INTERNATIONAL SYMPOSIUM ON ORGANIC MATTER MANAGEMENT AND COMPOST USE
   IN HORTICULTURE}},
