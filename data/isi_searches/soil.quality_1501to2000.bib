@article{ ISI:000341836700011,
Author = {Singh, R. N. and Tripathi, Satyendra and Sharma, Shaishav and Choudhary,
   Gaurav},
Title = {{Influence of Biodiesel Effluent on Physico-chemical Characteristics of
   Black Soil: a Laboratory Study}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{2014}},
Volume = {{225}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{Biodiesel has proven to be a reasonable substitute to petroleum diesel
   owing to continuous depletion and pollution caused by the latter. The
   uncomplicated process of production of fatty acid methyl ester or
   biodiesel as it is commercially known has made it an even better
   substitute of fossil diesel. The preparation of biodiesel involves the
   use of alcohol (methanol) and hydroxides (NaOH). There is a possibility
   that some of these compounds remain unreacted and needs to be washed
   with water. This residue water containing alcohol and hydroxide residues
   if discharged in the soil may affect its quality. This research paper
   deals with the effect of biodiesel effluent on various physicochemical
   properties of soil. The result of the research proves that the biodiesel
   effluent if discharged in soil will degrade soil quality.}},
