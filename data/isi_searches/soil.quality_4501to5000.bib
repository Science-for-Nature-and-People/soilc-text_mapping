@article{ ISI:000266240400006,
Author = {Rodrigues, Sonia and Urquhart, Graham and Hossack, Iain and Pereira, Ma
   Eduarda and Duarte, Armando C. and Davidson, Christine and Hursthouse,
   Andrew and Tucker, Peter and Roberston, Douglas},
Title = {{The influence of anthropogenic and natural geochemical factors on urban
   soil quality variability: a comparison between Glasgow, UK and Aveiro,
   Portugal}},
Journal = {{ENVIRONMENTAL CHEMISTRY LETTERS}},
Year = {{2009}},
Volume = {{7}},
Number = {{2}},
Pages = {{141-148}},
Month = {{JUN}},
Abstract = {{As part of a harmonised assessment of urban soils
   (http://www.urbsoil.paisley.ac.uk), we investigated the variability of
   metal content in soils from Aveiro (Portugal) and Glasgow (UK). Samples
   were collected from parks and other public open spaces in each city.
   Metal content (Al, Ca, Cr, Cu, Fe, Mn, Ni, Pb and Zn) and basic soil
   parameters (texture, CEC, pH, organic matter) were determined and data
   investigated using principal component analysis (PCA). The two cities
   differ in absolute levels of metal content reflecting industrial and
   historical development. Factors identified by PCA included anthropogenic
   (Cu, Pb, Zn), soil properties and geology, which explain variability
   when data were assessed based on metal content, soil properties and land
   use. This study highlights the contribution from geological background
   even in strongly urbanised environments.}},
Publisher = {{SPRINGER HEIDELBERG}},
Address = {{TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY}},
Type = {{Article}},
Language = {{English}},
