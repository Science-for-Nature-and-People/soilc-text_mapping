@article{ ISI:000403353500014,
Author = {Bakelaar, Joshua and Neher, Deborah A. and Gilker, Rachel},
Title = {{Minimal soil quality impact by cold season pasture management in Vermont}},
Journal = {{CANADIAN JOURNAL OF SOIL SCIENCE}},
