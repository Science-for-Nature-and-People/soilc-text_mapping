@article{ ISI:000285070900002,
Author = {Recatala, L. and Sanchez, J. and Arbelo, C. and Sacristan, D.},
Title = {{Testing the validity of a Cd soil quality standard in representative
   Mediterranean agricultural soils under an accumulator crop}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
