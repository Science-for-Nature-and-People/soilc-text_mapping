@article{ ISI:000273582400034,
Author = {Pignataro Netto, Iara Terezinha and Kato, Eiyti and Goedert, Wenceslau
   J.},
Title = {{PHYSICAL AND CHEMICAL PROPERTIES OF AN OXISOL CULTIVATED UNDER PASTURES
   WITH DISTINCT USE HISTORY}},
