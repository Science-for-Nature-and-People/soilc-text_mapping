@article{ ISI:000269811500048,
Author = {Nicolaescu, Marioara and Lupascu, Naliana and Chirila, Elisabeta},
Title = {{LAND DEGRADATION AND DESERTIFICATION RISK IN DOBROGEA REGION}},
Journal = {{ENVIRONMENTAL ENGINEERING AND MANAGEMENT JOURNAL}},
