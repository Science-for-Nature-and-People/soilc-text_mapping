@article{ ISI:000350217700056,
Author = {Gunal, Hikmet and Korucu, Tayfun and Birkas, Marta and Ozgoz, Engin and
   Halbac-Cotoara-Zamfir, Rares},
Title = {{Threats to Sustainability of Soil Functions in Central and Southeast
   Europe}},
