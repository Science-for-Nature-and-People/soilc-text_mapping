@article{ ISI:000249785400007,
Author = {Turczynowicz, Leonid and Robinson, Neville I.},
Title = {{Exposure assessment modeling for volatiles - Towards an Australian
   indoor vapor intrusion model}},
Journal = {{JOURNAL OF TOXICOLOGY AND ENVIRONMENTAL HEALTH-PART A-CURRENT ISSUES}},
Year = {{2007}},
Volume = {{70}},
