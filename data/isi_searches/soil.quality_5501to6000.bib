@article{ ISI:000232790500007,
Author = {Hu, ZY and Kuang, YQ and Wu, ZF and Liu, CS and Huang, NS and Zhu, ZY
   and Luo, W and Ouyang, T},
Title = {{Environmental quality and its impact on the local economy: A case study
   from Dongguan, South China}},
Journal = {{INTERNATIONAL JOURNAL OF SUSTAINABLE DEVELOPMENT AND WORLD ECOLOGY}},
Year = {{2005}},
Volume = {{12}},
Number = {{3}},
Pages = {{291-299}},
Month = {{SEP}},
Abstract = {{Industries that pollute the environment developed in the early 1990s in
   Dongguan City, especially those in the middle and lower valley of the
   Dongjiang River (waterside area) have seriously affected water and air
   quality and hence have affected soil quality through polluted water
   irrigation and precipitation. Investigation of the economic growth and
   environmental pollution in the 32 township units in the City from 1990
   to 2000 also reveals that environmental pollution has affected the
   history of the development of the towns or townships in the waterside
   region in the delta of the River Dongjiang, especially the towns near
   the river mouth, and has prevented their industry from upgrading and
   hence has slowed down their economic growth. The impact of environmental
   pollution on economic growth is most severe in the towns along the lower
   valley, or in down-wind regions. In contrast, economic development in
   the protected area of the upper valley of the waterside region indicates
   that environmental protection that strictly controls the discharge of
   pollutants does not restrict the development of the local economy.}},
