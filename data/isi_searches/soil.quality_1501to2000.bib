Title = {{Effects of Long-term Irrigation with Water from Wastewater Receiving
   River on Soil Chemical and Microbiological Properties in Hebei, China}},
Booktitle = {{PROCEEDINGS OF THE 2015 AASRI INTERNATIONAL CONFERENCE ON CIRCUITS AND
   SYSTEMS (CAS 2015)}},
Series = {{ACSR-Advances in Comptuer Science Research}},
