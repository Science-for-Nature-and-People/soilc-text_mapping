@inproceedings{ ISI:000170231900033,
Author = {Kendle, AD and Rose, JE},
Editor = {{Brundu, G and Brock, J and Camarda, I and Child, L and Wade, M}},
Title = {{Invasive plants on land recovering from desertification on Saint Helena
   Island}},
Booktitle = {{PLANT INVASIONS: SPECIES ECOLOGY AND ECOSYSTEM MANAGEMENT}},
Year = {{2001}},
Pages = {{311-318}},
Note = {{5th International Conference on Ecology of Invasive Alien Plants,
   SARDINIA, ITALY, OCT 13-16, 1999}},
Abstract = {{The vulnerability of small islands to invasion by non-native species is
   well known and is almost invariably seen as an undesirable trend that
   must be resisted and ideally reversed. However many islands are heavily
   modified systems where: the environmental conditions have changed, and
   where new demands are made on the biota. St Helena has undergone massive
   environmental degeneration and change and provides an example where new
   colonising species are both inevitable and sometimes beneficial. A
   review of the major colonists shows that many may lead to recovery of
   landuse functions or soil quality. The difficulties involved in control
   are also apparent. Alien status and a tendency for invasiveness are not
   in themselves adequate criteria for defining problem species in such a
   damaged system where revegetation is a priority. Throughout the world
   plant distribution changes will increasingly reflect environmental
   change and degradation rather than being causal agents of such
   degradation. More focused functional criteria are needed to identify
   where control measures must be targetted.}},
