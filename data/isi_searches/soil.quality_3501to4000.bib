@article{ ISI:000291840300010,
Author = {Riffaldi, Riccardo and Cardelli, Roberto and Saviozzi, Alessandro},
Title = {{SOIL QUALITY UNDER THE GIANT REED (ARUNDO DONAX L.) CROP COMPARED WITH A
   CROPPING SEQUENCE AND A NATURAL GRASSLAND}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2011}},
Volume = {{20}},
Number = {{6A}},
Pages = {{1535-1540}},
Abstract = {{The objective of this study was to determine, on the same pedological,
   topographic and climatic conditions, the effect of a continuous giant
   reed cropping (GR), a cropping sequence (CS) as well as of untilled
   native grassland (NO) on some soil quality characteristics. Between the
   parameters examined, total soil organic carbon (TOC), light fraction
   carbon (LFC), dissolved organic carbon (DOC), cumulative mineralized
   carbon (Cm), microbial biomass carbon (MBC), basal respiration,
   metabolic quotient (qCO(2)), potentially mineralizable carbon (C(0)),
   dehydrogenase (DHA), catalase (CA) and B-glucosidase (GL) activities, as
   well as fluorescein diacetate hydrolysis (FDA), biological index of
   fertility (BIF) and initial potential rate of carbon mineralization
   (C(0).k), were higher in the continuous giant reed cropping than in the
   cropping sequence. The C mineralization rate (k) was lower in GR than in
   CS, while the metabolic potential (MP) was similar in the two sites.
   Concerning the difference between OR and NO, the latter being considered
   as a reference or baseline in evaluating attributes of soil quality, OR
   showed higher values for TUC, LFC, DOC and catalase activity. TOC and
   catalase, also for their analytical simplicity, may be suggested as the
   most suitable soil biochemical indicators for differentiating the
   effects of different crop management systems.}},
