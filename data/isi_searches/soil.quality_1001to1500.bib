@article{ ISI:000360703100013,
Author = {Baldantoni, Daniela and Bellino, Alessandro and Morra, Luigi and Alfani,
   Anna},
Title = {{Compost Amendment Enhances Natural Revegetation of a Mediterranean
   Degraded Agricultural Soil}},
Journal = {{ENVIRONMENTAL MANAGEMENT}},
