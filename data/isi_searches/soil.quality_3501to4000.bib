@article{ ISI:000289820400010,
Author = {Gielen, Gerty J. H. P. and Clinton, Peter W. and Van den Heuvel, Michael
   R. and Kimberley, Mark O. and Greenfield, Laurie G.},
Title = {{INFLUENCE OF SEWAGE AND PHARMACEUTICALS ON SOIL MICROBIAL FUNCTION}},
