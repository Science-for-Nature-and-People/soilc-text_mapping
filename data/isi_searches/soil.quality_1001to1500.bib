@article{ ISI:000375333900001,
Author = {Unver, M. C. and Kucuk, M. and Tufekcioglu, A.},
Title = {{INFLUENCE OF LAND USE TYPE ON SOIL RESPIRATION RATES IN ARTYIN, TURKEY}},
Journal = {{OXIDATION COMMUNICATIONS}},
