Title = {{The influence of soil and weather conditions, agronomic measures and
   cultivars on common scab incidence on potato tubers}},
Booktitle = {{PROCEEDINGS OF THE SYMPOSIUM ON THE BIOTIC ENVIRONMENT OF AGRICULTURE
   AND DISEASE THREATS TO PLANTS}},
Year = {{1993}},
Pages = {{393-399}},
