@article{ ISI:000233211300003,
Author = {de Lima, HV and de Lima, CLR and Leao, TP and Cooper, M and da Silva, AP
   and Romero, RE},
Title = {{Agricultural machinery traffic and alterations in biopores under an
   orange orchard}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
