@article{ ISI:000395185200007,
Author = {Sparda, Andrew and Miller, Robert O. and Anderson, George and Hsieh,
   Yuch-Ping},
Title = {{Real-Time Soil CO2 Respiration Rate Determination and the Comparison
   between the Infrared Gas Analyzer and Microrespirometer (MicroRes((R)))
   Methods}},
