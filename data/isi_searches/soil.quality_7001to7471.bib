@article{ ISI:000075961100019,
Author = {Roberts, RK and Larson, JA and Tyler, DD and Duck, BN and Dillivan, KD},
Title = {{Economic analysis of the effects of winter cover crops on no tillage
   corn yield response to applied nitrogen}},
Journal = {{JOURNAL OF SOIL AND WATER CONSERVATION}},
Year = {{1998}},
Volume = {{53}},
Number = {{3}},
Pages = {{280-284}},
Abstract = {{The objective was to evaluate how cover crops affect profit-maximising
   nitrogen rates, corn yields, and net revenues for no tillage corn
   production in West Tennessee. Yield data for various applied nitrogen
   rates during 1986 through 1995 were used to estimate corn yield response
   functions for hairy vetch, crimson clover, winter wheat, and no cover
   alternatives. Profit-maximizing nitrogen rates, yield;, rests of
   production, and net revenues were then calculated. The vetch cover
   provided the highest profit-maximizing corn yields and net revenues and
   the lowest profit-maximizing applied nitrogen rates. The apposite was
   true for wheat. Clover provided lower net revenues than the no cover
   alternative.}},
