@article{ ISI:000306583700023,
Author = {Cachada, A. and Pato, P. and Rocha-Santos, T. and Ferreira da Silva, E.
   and Duarte, A. C.},
Title = {{Levels, sources and potential human health risks of organic pollutants
   in urban soils}},
Journal = {{SCIENCE OF THE TOTAL ENVIRONMENT}},
Year = {{2012}},
