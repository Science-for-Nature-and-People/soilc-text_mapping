@incollection{ ISI:000328215600006,
Author = {Ayuke, Fredrick O. and Karanja, Nancy K. and Okello, Julius J. and
   Wachira, Peter M. and Mutua, Gerald K. and Lelei, David K. and Gachene,
   Charles K. K.},
Editor = {{Hester, RE and Harrison, RM}},
Title = {{Agrobiodiversity and Potential Use for Enhancing Soil Health in Tropical
   Soils of Africa}},
Booktitle = {{SOILS AND FOOD SECURITY}},
