@article{ ISI:000307130300045,
Author = {Belanger, Valerie and Vanasse, Anne and Parent, Diane and Allard, Guy
   and Pellerin, Doris},
Title = {{Development of agri-environmental indicators to assess dairy farm
   sustainability in Quebec, Eastern Canada}},
Journal = {{ECOLOGICAL INDICATORS}},
Year = {{2012}},
