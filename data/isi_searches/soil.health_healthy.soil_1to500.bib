@article{ ISI:000286569100011,
Author = {Rajarathinam, A. and Khokhar, A. N. and Dixit, S. K.},
Title = {{STATISTICAL MODELLING ON GROUPING VILLAGES OF KADANA TALUKA BASED ON
   SOIL PARAMETERS}},
Journal = {{INTERNATIONAL JOURNAL OF AGRICULTURAL AND STATISTICAL SCIENCES}},
Year = {{2010}},
Volume = {{6}},
Number = {{2}},
Pages = {{423-430}},
Month = {{DEC}},
Abstract = {{An empirical investigation was carried out to study the pattern of
   variability of five soil parameters simultaneously across the villages
   and to group these villages having same pattern by considering data of
   47 villages of Kadana taluka (Panchmahal district) of Gujarat state. The
   data on five soil parameters viz., pH, Electrical Conductivity (EC),
   Organic Carbon (OC), available Phosphorus (P) and Potassium (K) were
   obtained from Soil Health Card (SHC) scheme. These soil parameters were
   subjected to various statistical analysis. The Analysis of Variance
   (ANOVA) test indicated that the variation due to pH, EC, OC, P and K
   among the villages were highly significant and it indicated that the
   individual parameters were significantly different across the villages.
   The Multivariate Analysis of Variance (MANOVA) test revealed a
   significant variability pattern between the villages when all the five
   soil parameters were considered simultaneously. Though all the soil
   parameters were found individually significant (ANOVA) as well as when
   considered simultaneously (MANOVA), the Ward's clustering technique
   considered only OC and EC for clustering. Accordingly three clusters
   were formed such that there was homogeneity within the clusters and
   heterogeneity between the clusters. These grouping would be helpful for
   preparing fertility maps as well as for implementing any soil
   improvement programme effectively and efficiently.}},
