@article{ ISI:000310307900009,
Author = {Mueller, Lothar and Schindler, Uwe and Shepherd, T. Graham and Ball,
   Bruce C. and Smolentseva, Elena and Hu, Chunsheng and Hennings, Volker
   and Schad, Peter and Rogasik, Jutta and Zeitz, Jutta and Schlindwein,
   Sandro L. and Behrendt, Axel and Helming, Katharina and Eulenstein,
   Frank},
Title = {{A framework for assessing agricultural soil quality on a global scale}},
Journal = {{ARCHIVES OF AGRONOMY AND SOIL SCIENCE}},
Year = {{2012}},
Volume = {{58}},
Number = {{1, SI}},
Pages = {{S76-S82}},
Note = {{International Conference on Soil Fertility and Soil Productivity,
   Humboldt Univ, Berlin, GERMANY, MAR 17-20, 2010}},
Organization = {{Int Union Soil Sci (IUSS); European Council of Soil Sci Soc; European
   Soc Soil Conservat}},
Abstract = {{This paper provides information about a novel approach of rating
   agricultural soil quality (SQ) and crop yield potentials consistently
   over a range of spatial scales. The Muencheberg Soil Quality Rating is
   an indicator-based straightforward overall assessment method of
   agricultural SQ. It is a framework covering aspects of soil texture,
   structure, topography and climate which is based on 8 basic indicators
   and more than 12 hazard indicators. Ratings are performed by visual
   methods of soil evaluation. A field manual is then used to provide
   ratings from tables based on indicator thresholds. Finally, overall
   rating scores are given, ranging from 0 (worst) to 100 (best) to
   characterise crop yield potentials. The current approach is valid for
   grassland and cropland. Field tests in several countries confirmed the
   practicability and reliability of the method. At field scale, soil
   structure is a crucial, management induced criterion of agricultural SQ.
   At the global scale, climate controlled hazard indicators of drought
   risk and soil thermal regime are crucial for SQ and crop yield
   potentials. Final rating scores are well correlated with crop yields. We
   conclude that this system could be evolved for ranking and controlling
   agricultural SQ on a global scale.}},
