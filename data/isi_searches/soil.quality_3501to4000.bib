@article{ ISI:000286992700010,
Author = {Tondoh, Jerome E. and Guei, Arnauth M. and Csuzdi, Csaba and Okoth,
   Peter},
Title = {{Effect of land-use on the earthworm assemblages in semi-deciduous
   forests of Central-West Ivory Coast}},
