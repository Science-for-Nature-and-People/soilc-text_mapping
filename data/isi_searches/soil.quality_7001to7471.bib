@article{ ISI:A1994QG30300007,
Author = {HURD, BH},
Title = {{YIELD RESPONSE AND PRODUCTION RISK - AN ANALYSIS OF INTEGRATED
   PEST-MANAGEMENT IN COTTON}},
Journal = {{JOURNAL OF AGRICULTURAL AND RESOURCE ECONOMICS}},
Year = {{1994}},
Volume = {{19}},
Number = {{2}},
Pages = {{313-326}},
Month = {{DEC}},
Abstract = {{Production uncertainty is commonly believed to be an impediment to the
   adoption of less pesticide-intensive methods in agriculture such as
   integrated pest management (IPM).  To investigate the effects of pest
   control inputs on yields and yield variability, data from a
   cross-section of San Joaquin Valley cotton producers were analyzed in a
   heteroskedastic production model.  The results suggest that yields are
   increasing with soil quality, crop rotation, frequency of field
   monitoring, and the use of independent pest control advisors.  Yield
   variability was not found to be significantly affected by production
   inputs, including pesticides and IPM practices with the exception of
   frequent contact with extension farm advisors which was found to
   contribute to reduced yield variability.}},
