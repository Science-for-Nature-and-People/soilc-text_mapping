@article{ ISI:000244766400039,
Author = {Bedini, Stefano and Avio, Luciano and Argese, Emanuele and Giovannetti,
   Manuela},
Title = {{Effects of long-term land use on arbuscular mycorrhizal fungi and
   glomalin-related soil protein}},
