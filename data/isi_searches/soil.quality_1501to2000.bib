@article{ ISI:000354156600004,
Author = {Dindaroglu, Turgay},
Title = {{RESISTANCE TO THE RECLAMATION OF ENVIRONMENTALLY SENSITIVE AREAS THROUGH
   THE ESTABLISHMENT OF A NEW FOREST ECOSYSTEM}},
