@inproceedings{ ISI:000183251800111,
Author = {Owens, VN and Doolittle, JJ},
Editor = {{Phillips, M}},
Title = {{Establishing alfalfa on high clay soils after application of municipal
   solid waste}},
Booktitle = {{AMERICAN FORAGE AND GRASSLAND COUNCIL, VOL 9, PROCEEDINGS}},
Series = {{PROCEEDINGS OF THE FORAGE AND GRASSLAND CONFERENCE}},
Year = {{2000}},
Volume = {{9}},
Pages = {{251-255}},
Note = {{37th North American Alfalfa Improvement Conference, MADISON, WI, JUL
   16-19, 2000}},
Organization = {{Amer Forage \& Grassland Council; Wisconsin Forage \& Grassland Council;
   N Amer Alfalfa Improvement Conf}},
Abstract = {{Alfalfa is difficult to establish in much of western South Dakota where
   the environment is harsh and the soils tend to crust after tillage due
   to their high clay content. Our objectives were to determine whether
   alfalfa establishment could be improved on land treated with composted
   municipal solid waste (MSW) or MSW co-composted with organic biosolids
   (MSW-Bio) from a wastewater treatment facility, and to evaluate the
   effects of these materials on soil quality. Baseline soil data were
   collected from all plots in the fall before application of MSW.
   Composted material was incorporated into the soil at a targeted rate of
   0, 40, and 80 ton acre(-1) the fall or spring before planting. The
   number of alfalfa plants in a 1-ft(2) area was determined in three areas
