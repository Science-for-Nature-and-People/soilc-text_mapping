@inproceedings{ ISI:000323183500094,
Author = {Ding, Guo-hua and Liu, Xiao-liang and Ma, Dian-rong and Wang, Xiao-xue
   and Yang, Guang and Gao, Ming-chao and Gao, Qi and Sun, Jian and Chen,
   Wen-fu},
Editor = {{Sun, T}},
Title = {{Responses of Weedy Rice to Drought Stress at Germination and Seedling
   Stages}},
Booktitle = {{ENERGY ENGINEERING AND ENVIRONMENTAL ENGINEERING, PTS 1AND 2}},
Series = {{Applied Mechanics and Materials}},
Year = {{2013}},
Volume = {{316-317}},
