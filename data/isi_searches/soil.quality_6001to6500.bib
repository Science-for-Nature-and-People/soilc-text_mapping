@article{ ISI:000172932300005,
Author = {Zalidis, G and Stamatiadis, S and Takavakoglou, V and Eskridge, K and
   Misopolinos, N},
Title = {{Impacts of agricultural practices on soil and water quality in the
   Mediterranean region and proposed assessment methodology}},
Journal = {{AGRICULTURE ECOSYSTEMS \& ENVIRONMENT}},
Year = {{2002}},
Volume = {{88}},
Number = {{2, SI}},
