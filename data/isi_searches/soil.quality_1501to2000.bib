@article{ ISI:000348065500028,
Author = {Armengot, Laura and Berner, Alfred and Blanco-Moreno, Jose M. and
   Maeder, Paul and Sans, F. Xavier},
Title = {{Long-term feasibility of reduced tillage in organic farming}},
Journal = {{AGRONOMY FOR SUSTAINABLE DEVELOPMENT}},
