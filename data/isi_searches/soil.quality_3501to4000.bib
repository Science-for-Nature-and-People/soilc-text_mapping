@article{ ISI:000297793900003,
Author = {Drescher, Marta Sandra and Moreira Rovedder, Ana Paula and Antoniolli,
   Zaida Ines and Foletto Eltz, Flavio Luiz and Drescher, Gerson Laerson},
Title = {{EPIGEIC FAUNA IN PRODUCTION SYSTEMS OF Nicotiana tabacum L.}},
