Title = {{PERSPECTIVES OF INCREASING BIODIESEL PRODUCTION THROUGH INTRODUCTION OF
   JATROPHA CURCAS L. INTO SUBSISTENCE FARMING SYSTEMS: EXPERIENCE FROM
   NORTH SUMATRA, INDONESIA}},
Booktitle = {{11TH INTERNATIONAL SCIENTIFIC CONFERENCE ON ENGINEERING FOR RURAL
   DEVELOPMENT, VOL 11}},
Series = {{Engineering for Rural Development}},
Year = {{2012}},
Pages = {{417-422}},
