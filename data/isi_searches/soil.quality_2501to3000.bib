@article{ ISI:000316514300007,
Author = {Bi Chun-Juan and Chen Zhen-Lou and Wang Jun and Zhou Dong},
Title = {{Quantitative Assessment of Soil Health Under Different Planting Patterns
   and Soil Types}},
