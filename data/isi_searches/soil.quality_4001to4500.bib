@article{ ISI:000281285800003,
Author = {Taylor, M. D. and Kim, N. D. and Hill, R. B. and Chapman, R.},
Title = {{A review of soil quality indicators and five key issues after 12 yr soil
   quality monitoring in the Waikato region}},
