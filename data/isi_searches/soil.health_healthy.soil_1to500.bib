@article{ ISI:000077955400018,
Author = {Annadurai, K and Palaniappan, S},
Title = {{Phosphorus management with diammonium phosphate spray on productivity of
   lowland transplanted rice (Oryza sativa)}},
Journal = {{INDIAN JOURNAL OF AGRONOMY}},
Year = {{1998}},
Volume = {{43}},
Number = {{2}},
Pages = {{278-283}},
Month = {{JUN}},
Abstract = {{An experiment was conducted during June 1993-February 1994 to study the
   effect of phosphorus levels and 2\% diammonium phosphate (DAP) spray on
   lowland rice (Oryza sativa L.). Application of P at recommended dose (38
   kg P2O5/ha) with 2\% DAP spray given thrice at boot leaf, 50\% flowering
   and post-milk stages resulted in higher rice grain productivity and
   profitability. Considering soil health and increase in P fertilizer
   price, the farmer may choose to apply P at 50\% of recommended dose (19
   kg P2O5/ha) with 2\% DAP spray twice at boot-leaf stage and 50\%
   flowering or at post-milk stage if capital is constraint for the farmer.
   Based on the previous season result, test verification trial was
   conducted in the winter (rabi) season with `IR 20' rice as test variety.
   Similar trend was also observed in the test verification trial. The
   effect on P uptake at different stages of crop growth and economics were
   also recorded.}},
