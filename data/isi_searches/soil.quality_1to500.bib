@article{ ISI:000399966600060,
Author = {Sun, Caixia and Xiang, Shuo and Zhang, Qi and Xiao, Wendan and Xu, Ping},
Title = {{HEAVY METAL CONTENT IN BR CCOLI (BRASSICA OLERACEA L. VAR. ITALICA
   PLENCK) AND POTENTIAL HEALTH RISK IN ZHEJIANG PROVINCE, CHINA}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2017}},
Volume = {{26}},
Number = {{3}},
Pages = {{2340-2347}},
Abstract = {{Broccoli is widely planted in Zhejiang Province, China, an area with
   heavy metal polluted soils due to industrial operations and rapid
   urbanization. In this study, we investigated heavy metal pollution in
   broccoli and the corresponding production soils in five cities
   (Hangzhou, Jiaxing, Ningbo, Shaoxing, and Taizhou) in Zhejiang Province.
   A total of 28 soil and 112 broccoli root, stem, leaf, and flower samples
   were analyzed for heavy metal (Pb, Cd, Hg, and As) content. According to
   the maximum levels of contaminants in foods as determined by the
   National Health Agency of China (GB2762-2012), the mean levels of Pb,
   Cd, Hg, and As in broccoli were within the allowable limits, except for
   37 samples in which Pb levels exceeded the threshold of 0.3 mg kg(-1).
   Assessment of soil pollution showed that the mean levels of Pb, Cd, and
   As met the Chinese Soil Quality Criterion (GB15618-1995, Level II),
   except for four soil samples from Ningbo and Shaoxing that exceeded the
   maximum allowable level of Hg. The exposure assessment showed that the
   levels of Pb, Cd, Hg, and As in broccoli roots, stems, flowers, and
   leaves were safe for human consumption, while the upper 97.5\% limit of
   Pb content indicated a potential health risk. Overall, it is suggested
   that more attention should be paid to heavy metal pollution in broccoli,
   and regular monitoring is recommended for the production areas in
   Zhejiang Province.}},
