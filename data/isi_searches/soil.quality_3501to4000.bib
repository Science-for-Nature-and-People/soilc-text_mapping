@article{ ISI:000288252700003,
Author = {Zhang, Jian and Pu, Lijie and Peng, Buzhuo and Gao, Zhonggui},
Title = {{The impact of urban land expansion on soil quality in rapidly urbanizing
   regions in China: Kunshan as a case study}},
Journal = {{ENVIRONMENTAL GEOCHEMISTRY AND HEALTH}},
