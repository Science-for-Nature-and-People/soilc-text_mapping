@inproceedings{ ISI:000174171700061,
Author = {Truman, CC and Shaw, JN and Reeves, DW},
Editor = {{Ascough, JC and Flanagan, DC}},
Title = {{Tillage effects on rainfall partitioning, sediment yield, and
   facilitated agrichemical transport potential}},
Booktitle = {{SOIL EROSION RESEARCH FOR THE 21ST CENTURY, PROCEEDINGS}},
Year = {{2001}},
Pages = {{237-240}},
Note = {{International Symposium on Soil Erosion Research for the 21st Century,
   HONOLULU, HI, JAN 03-05, 2001}},
Organization = {{Amer Soc Agr Engineers}},
Abstract = {{Coastal Plain soils located in the southeastern U.S. have been
   intensively cropped and traditionally managed under conventional tillage
   practices. They are susceptible to erosion, resulting in transport of
   pollutants to off-site water supplies through colloidal facilitated
   transport of agrichemicals. The objective of this study was to quantify
   rainfall partitioning, sediment delivery and characterization, and
   enrichment of clays between sediments and in situ soils from a Typic
   Hapludult loamy sand managed under conventional-till (CT), no-till (NT),
   and para-till systems. Plots (1 m(2)) established on each tillage system
   treatment were exposed to simulated rainfall for 2 h with an intensity
   of 50 nun h(-1). Runoff and sediment delivery were continuously measured
   from flat, level-sloping field plots (slope-1\%). More runoff (45\% vs.
   72\%) and sediment losses Q fold) occurred from CT plots compared to NT
   plots, Removing residue from NT and CT plots increased runoff (38-48\%)
   and sediment (2-5 fold) losses compared to corresponding plots with
   residue. Para-tilling decreased runoff (by up to 50\%) and sediment (2
   fold) losses compared to non-para-tilled plots. Amount of water
   dispersible clay (WDC) was correlated with \%OC (W=0.76), which was a
   function of tillage treatment. Sediments were enriched in quartz and
   depleted with respect to kaolinite as compared to in situ soils.}},
