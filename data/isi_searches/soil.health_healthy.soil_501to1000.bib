@article{ ISI:000303381300029,
Author = {Anjum, Reshma and Malik, Abdul},
Title = {{Mutagenicity assessment of contaminated soil in the vicinity of
   industrial area}},
Journal = {{ENVIRONMENTAL MONITORING AND ASSESSMENT}},
