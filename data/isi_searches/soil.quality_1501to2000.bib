@article{ ISI:000340300300008,
Author = {Araujo, Quintino R. and Fernandes, Cinira A. F. and Ribeiro, Daniel O.
   and Efraim, Priscilla and Steinmacher, Douglas and Lieberei, Reinhard
   and Bastide, Philippe and Araujo, Taiana G.},
Title = {{Cocoa Quality Index - A proposal}},
Journal = {{FOOD CONTROL}},
Year = {{2014}},
Volume = {{46}},
Pages = {{49-54}},
Month = {{DEC}},
Abstract = {{The definition of food quality has varied over time but has evolved so
   that current definitions are based on meeting customer demands. However,
   in order to better define food quality a more comprehensive evaluation
   based in key variables is required. This study aims to propose a Cocoa
   Quality Index (CQI). A better understanding of what affects cocoa
   quality will allow researchers and managers to improve their product,
   benefiting producers and consumers. The present analysis was based on
   known desired characteristics of the cocoa bean. Total fat, total
   acidity, total phenols, phenolic acids, organic acids, heavy metals,
   amino acids, caffeine, theobromine, pH, sugars and macro and
   micronutrients were the main variables included in the CQI for the
   Forastero cocoa beans. The analysis was run on beans from two separate
   sites in Bahia, an Oxisol and an Alfisol. CQI values from the two sites
   did not differ statistically, but the adopted qualitative classification
   identified that the Alfisol had a `good' CQI (0.703) while the Oxisol
   was classified as `regular' one (CQI = 0.652). The difference was due to
   the better edaphic characteristics of the Alfisol. Results suggested
   that improving the cocoa bean in Bahia will require that total fat,
   phenolic compounds and glucose be increases arid that total phenols,
   heavy metals, organic acids and caffeine be decreased. The proposed CQI
   method can be adapted to other agricultural products. (C)14 Elsevier
   Ltd. All rights reserved.}},
