@incollection{ ISI:000384347200009,
Author = {Igalavithana, Avanthi Deshani and Ok, Yong Sik and Usman, Adel R. A. and
   Al-Wabel, Mohammad I. and Oleszczuk, Patryk and Lee, Sang Soo},
