@article{ ISI:000298513900005,
Author = {Yang, Huimin and He, Shubin and Shen, Yuying and Wang, Xianzhi},
Title = {{Soil Nitrogen, Carbon and Phosphorus after Lucerne Conversion to Wheat
   or Fallow in the Loess Plateau of China}},
Journal = {{PHILIPPINE JOURNAL OF CROP SCIENCE}},
Year = {{2011}},
Volume = {{36}},
Number = {{3}},
Pages = {{28-33}},
Month = {{DEC}},
Abstract = {{The introduction of lucerne (Medicago sativa L.) into the agricultural
   systems on the Loess Plateau of China has proved to be an excellent way
   to improve the sustainability of agriculture and environment. Extensive
   investigation has revealed lucerne's role in affecting soil water
   status. However, further exploration of the changes in soil nitrogen
   (N), carbon (C) and phosphorus (P) after lucerne removal is still
   required. This study investigated the changes of such elements following
   4 years of lucerne performance to assess the influence of wheat
   cultivation or fallow after lucerne removal on soil fertility, compared
   with continuous lucerne. Soil mineral N, organic C, and available P were
   sensitive to cultivation techniques and years, while soil total N and
   total P were different between two soil layers. Soil mineral N and
   available P tended to fluctuate with years under continuous lucerne,
   while soil organic C increased with years in the shallower soils but
   decreased in the deeper soils. The operation of fallow after lucerne
   removal led to soil nitrate N and available P accumulation due to the
   lack of plant utilisation of nutrients potentially resulting in soil N
   and P leaching. Wheat cultivation after lucerne removal led to the
   depletion of soil mineral N in both soil layers. Although there was no
   consistent change, the results indicated that soil fertility can be
   enhanced within 4 years of lucerne performance, and the efficiency of
   nutrient utilisation may be promoted under wheat cultivation after
   lucerne removal.}},
