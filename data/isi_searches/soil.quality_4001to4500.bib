
@article{ ISI:000287160100002,
Author = {Louwagie, G. and Gay, S. H. and Sammeth, F. and Ratinger, T.},
Title = {{THE POTENTIAL OF EUROPEAN UNION POLICIES TO ADDRESS SOIL DEGRADATION IN
   AGRICULTURE}},
