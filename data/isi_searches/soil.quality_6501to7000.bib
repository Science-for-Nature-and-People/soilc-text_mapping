@inproceedings{ ISI:000189401900002,
Author = {Meyer, JH and Van Antwerpen, R},
Editor = {{Hogarth, DM}},
Title = {{Soil degradation as a factor in yield decline in the South African sugar
   industry}},
Booktitle = {{INTERNATIONAL SOCIETY OF SUGAR CANE TECHNOLOGISTS, VOL II, PROCEEDINGS}},
Year = {{2001}},
