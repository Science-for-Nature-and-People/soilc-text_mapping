@article{ ISI:000247673300017,
Author = {Heerink, Nico and Qu, Futian and Kuiper, Marijke and Shi, Xiaoping and
   Tan, Shuhao},
Title = {{Policy reforms, rice production and sustainable land use in China: A
   macro-micro analysis}},
Journal = {{AGRICULTURAL SYSTEMS}},
