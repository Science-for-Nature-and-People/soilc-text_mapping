@article{ ISI:000209543500006,
Author = {Fiorentino, Nunzio and Fagnano, Massimo and Adamo, Paola and
   Impagliazzo, Adriana and Mori, Mauro and Pepe, Olimpia and Ventorino,
   Valeria and Zoina, Astolfo},
Title = {{Assisted phytoextraction of heavy metals: compost and Trichoderma
   effects on giant reed (Arundo donax L.) uptake and soil N-cycle
   microflora}},
Journal = {{ITALIAN JOURNAL OF AGRONOMY}},
Year = {{2013}},
Volume = {{8}},
Number = {{4}},
Pages = {{244-254}},
Abstract = {{Little information is available as to the real effectiveness of the
   phytoextraction remediation technique, since laboratory experiments are
   still the most common way in which this is measured. Given this, an
   experiment on a cadmium-polluted soil was carried out in open field
   conditions in Southern Italy with the aim of assessing the growth and
   the phytoextraction potential of giant reed (Arundo donax L). Compost
   fertilisation and Trichoderma harzianum A6 inoculations were used to
   verify the possibility of increasing the metal uptake of the crop.
   Biomass yield of giant reed in the first growth season (average 12.8 Mg
   ha(-1)) was not affected by the Cd concentration in the soil and this
   increased significantly with compost fertilisation (13.8 Mg ha(-1)).
   Both compost fertilisation and T. harzianum inoculation increased
   cadmium uptake and translocation in leaves. Nitrifying bacteria was
   shown to be a useful tool to biomonitor soil quality. These results
   proved the suitability of the giant reed for assisted-phytoremedation
   with the use of compost fertilisation and T. harzianum.}},
