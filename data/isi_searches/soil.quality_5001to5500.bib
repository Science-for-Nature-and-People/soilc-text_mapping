Title = {{Ecology-related microbiological and biochemical parameters in assessing
   soils exposed to anthropogenic pollution}},
Booktitle = {{MULTIPLE STRESSORS: A CHALLENGE FOR THE FUTURE}},
Series = {{NATO Science for Peace and Security Series C-Environmental Security}},
Year = {{2007}},
Pages = {{409+}},
