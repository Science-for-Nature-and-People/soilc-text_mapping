@article{ ISI:000278583400014,
Author = {Fereidunian, A. and Lucas, C. and Lesani, H. and Rahmani, R. and Wymore,
   A. W.},
Title = {{A Policy-Driven Method for IT Infrastructure Selection in Power
   Distribution Automation System}},
Journal = {{INTERNATIONAL REVIEW OF ELECTRICAL ENGINEERING-IREE}},
