@article{ ISI:000289821300001,
Author = {Tesfahunegn, Gebreyesus Brhane and Tamene, Lulseged and Vlek, Paul L. G.},
Title = {{A participatory soil quality assessment in Northern Ethiopia's Mai-Negus
   catchment}},
