@article{ ISI:000235088200013,
Author = {Terra, JA and Shaw, JN and Reeves, DW and Raper, RL and van Santen, E
   and Schwab, EB and Mask, PL},
Title = {{Soil management and landscape variability affects field-scale cotton
   productivity}},
