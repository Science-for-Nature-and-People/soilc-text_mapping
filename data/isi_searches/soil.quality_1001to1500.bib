@article{ ISI:000354100800001,
Author = {Mondal, Tanushree and Datta, Jayanta Kumar and Mondal, Naba Kumar},
Title = {{Influence of indigenous inputs on the properties of old alluvial soil in
   a mustard cropping system}},
