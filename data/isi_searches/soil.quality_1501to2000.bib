@article{ ISI:000354156600010,
Author = {Mngadi, Sihle and Moodley, Roshila and Jonnalagadda, Sreekanth B.},
Title = {{ELEMENTAL DISTRIBUTION AND CHEMICAL COMPOSITION OF AMADUMBE (COLOCASIA
   ESCULENTA) LEAF AND IMPACT OF SOIL QUALITY}},
