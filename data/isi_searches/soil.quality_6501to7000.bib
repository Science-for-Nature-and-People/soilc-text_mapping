@article{ ISI:000169663700009,
Author = {Fowler, R and Rockstrom, J},
Title = {{Conservation tillage for sustainable agriculture - An agrarian
   revolution gathers momentum in Africa}},
Journal = {{SOIL \& TILLAGE RESEARCH}},
Year = {{2001}},
Volume = {{61}},
Number = {{1-2}},
Pages = {{93-107}},
