@article{ ISI:000301789000007,
Author = {Su Zhengan and Zhang Jianhui and Xiong Donghong and Liu Gangcai},
Title = {{Assessment of Soil Erosion by Compensatory Hoeing Tillage in a Purple
   Soil}},
Journal = {{JOURNAL OF MOUNTAIN SCIENCE}},
Year = {{2012}},
