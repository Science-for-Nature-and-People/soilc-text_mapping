@article{ ISI:000404385100025,
Author = {Pavao-Zuckerman, Mitchell A. and Sookhdeo, Christine},
Title = {{Nematode Community Response to Green Infrastructure Design in a Semiarid
   City}},
Journal = {{JOURNAL OF ENVIRONMENTAL QUALITY}},
