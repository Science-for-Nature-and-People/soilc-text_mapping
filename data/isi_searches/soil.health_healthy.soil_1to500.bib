@article{ ISI:000181351300033,
Author = {Stokes, J and Cody, J and Maheswaran, J},
Title = {{The application of wool scour waste for broadacre cropping}},
Journal = {{WOOL TECHNOLOGY AND SHEEP BREEDING}},
Year = {{2002}},
Volume = {{50}},
Number = {{4}},
Pages = {{749-753}},
Note = {{1st Sheep and Wool Conference, HAMILTON, AUSTRALIA, 2002}},
Abstract = {{Large volumes of wool scour waste are placed in Victorian landfills
   every year at a high cost. Wool scour sludges can be effectively
   composted by blending with other materials to produce a value-added
   product that is nutrient rich and an ideal soil ameliorant for
   increasing organic matter and conserving moisture.
   Past studies of the use of composted wool scour waste in horticultural
   production have shown significant improvements in crop production and
   soil structure after one application. It was envisaged that major
   benefits in soil health would also be gained in broadacre-cropping soils
   if ameliorated with composted wool scour waste.
   In 2001, a long-term field trial was established to investigate the
   effect of composted wool scour waste for broadacre cropping and to
   determine the optimal rates and frequency of compost application over
   several cropping seasons. The field study is being undertaken at
   Southern Farming Systems in Gnarwarre, 20km west of Geelong. Composted
   wool scour sludge was applied to raised beds at 10 different rates,
   replicated 6 times. A variety of soil and crop properties are being
   monitored throughout the trial. The results from the first year
   demonstrate the effectiveness of compost applications as a means of
   increasing soil organic matter and moisture retention, thereby improving
   conditions for crop root development.}},
