@article{ ISI:000377738500006,
Author = {Yin, Xiaogang and Olesen, Jorgen E. and Wang, Meng and Kersebaum,
   Kurt-Christian and Chen, Huang and Baby, Sanmohan and Ozturk, Isik and
   Chen, Fu},
Title = {{Adapting maize production to drought in the Northeast Farming Region of
   China}},
Journal = {{EUROPEAN JOURNAL OF AGRONOMY}},
