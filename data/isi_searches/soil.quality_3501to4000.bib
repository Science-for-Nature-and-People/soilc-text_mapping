@article{ ISI:000297834000002,
Author = {Fang, Xuan and Xue, Zhijing and Li, Bicheng and An, Shaoshan},
Title = {{Soil organic carbon distribution in relation to land use and its storage
   in a small watershed of the Loess Plateau, China}},
Journal = {{CATENA}},
