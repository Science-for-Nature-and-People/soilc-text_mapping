@article{ ISI:000361048300018,
Author = {Wegner, Brianna R. and Kumar, Sandeep and Osborne, Shannon L. and
   Schumacher, Thomas E. and Vahyala, Ibrahim E. and Eynard, Anna},
Title = {{Soil Response to Corn Residue Removal and Cover Crops in Eastern South
   Dakota}},
