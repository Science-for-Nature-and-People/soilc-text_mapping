@inproceedings{ ISI:000290182900011,
Author = {Wicki, Ludwik and Dudek, Hanna},
Editor = {{Ivans, U}},
Title = {{Factors Influencing Productivity of Cereals in Polish Agriculture}},
Booktitle = {{ECONOMIC SCIENCE FOR RURAL DEVELOPMENT: FINANCES, TAXES, INVESTMENT AND
   SUPPORT SYSTEMS}},
