@inproceedings{ ISI:000305515500052,
Author = {Zaccheo, P. and Crippa, L. and Orfeo, D.},
Editor = {{Carlile, WR and Coules, A}},
Title = {{Application of ISO 11269-1 Root Elongation Bioassay for Testing the
   Physical Properties of Growing Media}},
