@inproceedings{ ISI:000338036100012,
Author = {Rudow, Katja},
Book-Group-Author = {{Czech Univ Life Sci Prague, Fac Econ \& Management}},
Title = {{Less Favoured Area Payments in Germany- Impacts on the Environment}},
Booktitle = {{AGRARIAN PERSPECTIVES}},
