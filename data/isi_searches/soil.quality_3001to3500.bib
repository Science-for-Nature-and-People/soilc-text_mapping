@article{ ISI:000314086800011,
Author = {de Assis Junior, Raimundo Nonato and da Silva, Edison Fernandes},
Title = {{EFFECT OF THE WATER IRRIGATION QUALITY ON PHYSICAL PROPERTIES OF AN
   ENTISOL IN QUIXERE, CE - BRAZIL}},
Journal = {{REVISTA BRASILEIRA DE CIENCIA DO SOLO}},
