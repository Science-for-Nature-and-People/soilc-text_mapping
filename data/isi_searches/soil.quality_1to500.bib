@article{ ISI:000393658700010,
Author = {Burke, William J. and Jayne, Thom. S. and Black, J. Roy},
Title = {{Factors explaining the low and variable profitability of fertilizer
   application to maize in Zambia}},
Journal = {{AGRICULTURAL ECONOMICS}},
