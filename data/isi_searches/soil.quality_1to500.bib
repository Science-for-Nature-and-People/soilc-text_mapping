@article{ ISI:000399967900026,
Author = {Liu, Hanhu and Liu, Guanghui and Zhou, Yunfan and He, Chao},
Title = {{Spatial Distribution and Influence Analysis of Soil Heavy Metals in a
   Hilly Region of Sichuan Basin}},
