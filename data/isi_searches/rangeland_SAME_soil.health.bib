@article{ ISI:000354122700013,
Author = {Houston, Wayne A. and Wormington, Kevin R. and Black, Robert L.},
Title = {{Termite (Isoptera) diversity of riparian forests, adjacent woodlands and
   cleared pastures in tropical eastern Australia}},
Journal = {{AUSTRAL ENTOMOLOGY}},
